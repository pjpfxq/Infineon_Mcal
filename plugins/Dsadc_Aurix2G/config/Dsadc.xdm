                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f61c6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../DsadcFIR0FilterEnable = 'false') and (../DsadcFIR1FilterEnable = 'true'))"
                                       true="Since the FIR0 is disabled, FIR1 should also be disabled."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={C526AF96-C509-492c-805A-2C837525BB95}] DsadcFIR1FilterDecimationEnable [/cover] -->
                            <v:var name="DsadcFIR1FilterDecimationEnable" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the decimation rate of FIR1 filter.If selected as TRUE, then the filter decimates with a ratio 2:1"/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f62c6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../DsadcFIR1FilterEnable = 'false') and (../DsadcFIR1FilterDecimationEnable = 'true'))"
                                       true="Since the FIR1 is disabled, FIR1 should not decimate ."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={B2A1EE98-5E6C-4a8b-85E7-49831654B1F5}] DsadcPreFilterEnable [/cover] -->
                            <v:var name="DsadcPreFilterEnable" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the availability of Prefilter in the filter chain of the DSADC channel "/>
