                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f30c6ccc"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:da name="INVALID" type="Range">
                                      <a:tst expr="&lt;=63"/>
                                      <a:tst expr="&gt;=0"/>
                                    </a:da>
                                  </v:var>
                                  <!-- [cover parentID={E4019A17-6E2C-463d-A2AD-08033D3DAD88}] DsadcIntegrationCount [/cover] -->
                                  <v:var name="DsadcIntegrationCount" type="INTEGER">
                                    <a:a name="DESC" value="EN: This parameter defines the number of result values to be integrated. "/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f32c6ccc"/>
                                    <a:da name="DEFAULT" value="2"/>
                                    <a:da name="INVALID" type="Range">
                                      <a:tst expr="&lt;=64"/>
                                    <a:tst expr="&gt;=2"/>
                                    </a:da>
                                  </v:var>
                                </v:ctr>
