                              <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- [cover parentID={B6562ADF-DEB6-456c-A00F-FF5690061884}] DsadcOvershootCompensationEn [/cover] -->
                            <v:var name="DsadcOvershootCompensationEn" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the availability of Overshoot compensation block in the filter chain of the DSADC channel."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="dea982da-0cb2-4bda-8324-e6e21fb90b04"/>
                              <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- [cover parentID={38CADD6A-1EEA-4bf1-BB57-6966DF426EBE}] DsadcCICFilterDecimationFactor [/cover] -->
                            <v:var name="DsadcCICFilterDecimationFactor" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter defines the oversampling rate/Decimation factor for the CIC filter. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f64c6ccc"/>
                              <a:da name="DEFAULT" value="4"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&lt;=512"/>
                                <a:tst expr="&gt;=4"/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={9B06F8B4-5B31-40ea-89D4-6DFDEB5B8380}] DsadcCICFilterStartValue [/cover] -->
                            <v:var name="DsadcCICFilterStartValue" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter defines the starting value of decimation counter, when the CIC filter is started/restarted. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f66c6ccc"/>
                              <a:da name="DEFAULT" value="4"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&lt;=512"/>
                                <a:tst expr="&gt;=4"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(../DsadcCICFilterStartValue) > (../DsadcCICFilterDecimationFactor)"
                                       true="Value of DsadcCICFilterStartValue is set higher than the value of DsadcCICFilterDecimationFactor. This can lead to an CIC filter overflow"/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={0EA0609A-1AAA-46b5-8736-335E43C648F2}] DsadcAlternateServiceReq [/cover] -->
                            <v:var name="DsadcAlternateServiceReq" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter is used to generate alternate service request for any one of the following events, comparator event, time stamp event or alternate source."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f68c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_ALT_SERVICE_DISABLE"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_ALT_SERVICE_DISABLE</a:v>
                                <a:v>DSADC_COMPARATOR_EVENT</a:v>
                                <a:v>DSADC_TIMESTAMP_EVENT</a:v>
                                <a:v>DSADC_RESOLVER_EVENT</a:v>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={ECA2D8A7-21D2-4d9e-B9EA-5A25C28DB1B2}] DsadcOffsetCompFilterEnable [/cover] -->
                            <v:var name="DsadcOffsetCompFilterEnable" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter indicates availability of an Offset Compensation (IIR) filter (and its various operation modes) in Offset Compensation block."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f69c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_OFFCOMP_FILTER_DISABLE"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_OFFCOMP_FILTER_DISABLE</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_1</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_2</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_3</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_4</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_5</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_6</a:v>
                                <a:v>DSADC_OFFCOMP_FILTER_RATE_7</a:v>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={20C54EB1-829A-4c4b-964D-0FC4015DED74}] DsadcOffsetCompValueProtect [/cover] -->
                            <v:var name="DsadcOffsetCompValueProtect" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the protection of the Offset Compensation register from the calibration algorithm. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f6ac6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(((../DsadcOffsetCompFilterEnable != 'DSADC_OFFCOMP_FILTER_DISABLE') and (../DsadcOffsetCompValueProtect = 'false')) )"
                                       true=" Since the Offset compensation filter(high pass filter) has been enabled but the protection of the offset compensation value from calibration algorithm is still not enabled"/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={2250384B-F444-46ab-AA30-D53498D71EC3}] DsadcOffsetCompValue [/cover] -->
                            <v:var name="DsadcOffsetCompValue" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter defines the offset component value to be removed(subtracted) from each result from the filter chain. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f6bc6ccc"/>
                              <a:da name="DEFAULT" value="0"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&lt;=32767"/>
                                <a:tst expr="&gt;=-32768"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../DsadcOffsetCompFilterEnable != 'DSADC_OFFCOMP_FILTER_DISABLE') and (../DsadcOffsetCompValue != '0'))"
                                       true="Since Offset compensation filter has been enabled, the user is not allowed to enter a configuration for offset component value."/>
                              </a:da>
                            </v:var>

