                          <!-- [cover parentID={5B1C35F7-8434-4b87-AD30-8FFEB22A4665}] Support DMA mode irrespective of Access mode [/cover] -->
                          <v:var name="DsadcAccessMode" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter determines the access mode selection for DSADC Driver. The available access mode is depends on parameter DsdcTriggerMode.If the DsadcTriggerMode is configured as TRIGGER_MODE_NORMAL then DsadcAccessMode can be any inside the Range. If DsadcTriggerMode is Configured as TRIGGER_MODE_WINDOW then DsadcAccessMode can be DSADC_SINGLE_READ or DSADC_STRERAM_LINEAR_BUFFER or DSADC_DMA_ACCESS."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="c46bbfe5-3c7e-4b1b-9127-79334c04d53c"/>
                            <a:da name="DEFAULT" value="DSADC_SINGLE_READ"/>
                            <a:da name="RANGE">
                              <a:v>DSADC_SINGLE_READ</a:v>
                              <a:v>DSADC_CIRCULAR_BUFFER</a:v>
                              <a:v>DSADC_STREAM_LINEAR_BUFFER</a:v>
                              <a:v>DSADC_DMA_ACCESS</a:v>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="((../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_WINDOW') and (. = 'DSADC_CIRCULAR_BUFFER'))"
                                     true="DsadcTriggerMode is Configured as TRIGGER_MODE_WINDOW then DsadcAccessMode can be DSADC_SINGLE_READ or DSADC_STRERAM_LINEAR_BUFFER or DSADC_DMA_ACCESS."/>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={64B6D529-CC6D-4ca3-AECD-5ADE1685C6D6}] DsadcTriggerMode  [/cover] -->
                          <v:var name="DsadcTriggerMode" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter is used to define the trigger mode. possbile conversion modes are DSADC_TRIGGER_MODE_NORMAL and DSADC_TRIGGER_MODE_WINDOW.
                                                        DSADC_TRIGGER_MODE_NORMAL: Conversion starts after calling Dsadc_StartModulation() API.
                                                        DSADC_TRIGGER_MODE_WINDOW: The result values are considered after Hardware trigger signal(GTM or ERU) is arrived."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="a87c6432-965e-4c08-9f97-ed59fe64506f"/>
                            <a:da name="DEFAULT" value="DSADC_TRIGGER_MODE_NORMAL"/>
                            <a:da name="RANGE">
                              <a:v>DSADC_TRIGGER_MODE_NORMAL</a:v>
                              <a:v>DSADC_TRIGGER_MODE_WINDOW</a:v>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={0ABC135A-03CD-415a-8FA7-5F4C5BB73CFA}] DsadcTimestampFeature [/cover] -->
                          <v:var name="DsadcTimestampFeature" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This Parameter is used to define the timestamp Functionality availability for the DSADC Channel.
                                                        DSADC_TIMESTAMP_ENABLED: Enable the Timestamp functionality.
                                                        DSADC_TIMESTAMP_DISABLED: Disable the Timestamp Functionality."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="25863155-48e3-438d-98dc-754126a66d0c"/>
                            <a:da name="DEFAULT" value="DSADC_TIMESTAMP_DISABLED"/>
                            <a:da name="RANGE">
                              <a:v>DSADC_TIMESTAMP_ENABLED</a:v>
                              <a:v>DSADC_TIMESTAMP_DISABLED</a:v>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="((../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_NORMAL') and (../DsadcAccessMode != 'DSADC_SINGLE_READ') and (. = 'DSADC_TIMESTAMP_ENABLED'))"
                                     true="Timestamp can be enabled only if the Trigger mode is configured as Window or Accessmode is single."/>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={2486C427-3E9A-4eea-A668-D86FBEA3F89A}] DsadcGateActiveLevel [/cover] -->
                          <v:var name="DsadcGateActiveLevel" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter is used to define the active level for the trigger gate signal."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="72bfe4df-c1de-4e91-8687-9bf5b7a9485c"/>
                            <a:da name="DEFAULT" value="DSADC_GATE_HIGH_LEVEL"/>
                            <a:da name="RANGE">
                              <a:v>DSADC_GATE_HIGH_LEVEL</a:v>
                              <a:v>DSADC_GATE_LOW_LEVEL</a:v>
                            </a:da>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_WINDOW'"/>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={DE7A2D2C-C272-44d5-90B8-3447C9129D14}] DsadcBufferFullNotification [/cover] -->
                          <v:var name="DsadcBufferFullNotification" type="FUNCTION-NAME">
                            <a:a name="DESC" value="EN: Callback function for buffer full event. The availablity of this parameter depends on DsadcAccessMode. If the DsadcAccessMode is configured as ACCESS_MODE_SINGLE OR ACCESS_MODE_CIRCULAR_BUFFER or DSADC_DMA_ACCESS then this parameter is not available."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ECUC:13cdc2fe-cf3c-4a68-93cb-a3676a293fd3"/>
                            <a:da name="DEFAULT" value="NULL_PTR"/>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../DsadcAccessMode = 'DSADC_STREAM_LINEAR_BUFFER'"/>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="((. = 'NULL')or(. = '') or (.=' '))" true="EN: DSADC - Invalid Configuration, value of NULL or empty field is not allowed."/>
                              <a:tst expr="((. != 'NULL_PTR') and (../DsadcAccessMode != 'DSADC_STREAM_LINEAR_BUFFER'))" true="EN: DsadcAccessMode is not Linear Buffer so the value should be NULL_PTR"/>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={6074B4B3-001C-46a0-9E85-729E8E680886}] DsadcNewResultNotification [/cover] -->
                          <v:var name="DsadcNewResultNotification" type="FUNCTION-NAME">
                            <a:a name="DESC" value="EN:Callback function for new result event."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ECUC:9918056e-9ee3-49ce-b4fc-d4b8fa37c2d9"/>
                            <a:da name="DEFAULT" value="NULL_PTR"/>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../DsadcAccessMode != 'DSADC_DMA_ACCESS'"/>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="((. = 'NULL')or(. = '') or (.=' '))" true="EN: DSADC - Invalid Configuration, value of NULL or empty field is not allowed."/>
                              <a:tst expr="((. != 'NULL_PTR') and (../DsadcAccessMode = 'DSADC_DMA_ACCESS'))" true="EN: DsadcAccessMode is DMA Access so the value should be NULL_PTR"/>
                            </a:da>
                          </v:var>
                          <!-- [cover parentID={715DDBDB-9440-45cb-AB69-9530B6984E09}] DsadcWindowCloseNotification [/cover] -->
                          <v:var name="DsadcWindowCloseNotification" type="FUNCTION-NAME">
                            <a:a name="DESC" value="EN:Callback function for window close event, the availability of this parameter depends on DsadcTriggerMode configured as DSADC_TRIGGER_MODE_NORMAL"/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ECUC:c7c6aad9-cd74-4df6-a2ec-b8d556521c92"/>
                            <a:da name="DEFAULT" value="NULL_PTR"/>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_WINDOW'"/>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="((. = 'NULL')or(. = '') or (.=' '))" true="EN: DSADC - Invalid Configuration, value of NULL or empty field is not allowed."/>
                              <a:tst expr="((. != 'NULL_PTR') and (../DsadcTriggerMode != 'DSADC_TRIGGER_MODE_WINDOW'))" true="EN: DsadcTriggerMode is not configured as TRIGGER_MODE_WINDOW. so the value should be NULL_PTR"/>
                            </a:da>
                          </v:var>

                          <!--********************************************************-->
                          <!--*****Dsadc Channel Modulator Configuration - Start **** -->
                          <!--********************************************************-->
                          <!-- [cover parentID={2784D793-9A03-462d-9F8E-BD48AB2948FB}] DsadcModulatorConfiguration [/cover] -->
                          <v:ctr name="DsadcModulatorConfiguration" type="IDENTIFIABLE">
                            <a:a name="UUID" value="6062ca80-76f1-43eb-8fae-6010059e2ccc"/>
                            <a:a name="DESC" value="EN: This container contains configuration parameters related to the On-chip modulator, input pin selection and modulator clock configuration."/>
                            <!-- [cover parentID={EBC634C6-6DD7-4ccc-BD82-274451148FFD}] DsadcAnalogClockSyncDelay [/cover] -->
                            <v:var name="DsadcAnalogClockSyncDelay" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter defines the delay in clock cycles after the sync signal provided from Phase synchronizer. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-44e9f55c6ccc"/>
                              <a:da name="DEFAULT" value="0"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&lt;=7"/>
                                <a:tst expr="&gt;=0"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../../../../DsadcGlobalConfiguration/DsadcSyncClockGen) = 'UNSYNCHRONIZED_MODE') and (../DsadcAnalogClockSyncDelay != '0')"
                                       true="Synchronized mode is not enabled, yet DsadcAnalogClockSyncDelay is configured for a value other than default."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={A8422D27-E1A5-46e2-85A1-C27CF47B8773}] DsadcClockDivider [/cover] -->
                            <v:var name="DsadcClockDivider" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines the divider factor, which defines the frequency of the modulator clock, which is derived from the peripheral clock "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e2f55c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_CLOCKDIVIDER_DIV4"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_CLOCKDIVIDER_DIV4</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV6</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV8</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV10</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV12</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV14</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV16</a:v>
                                <a:v>DSADC_CLOCKDIVIDER_DIV18</a:v>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={A308763A-99DC-450b-AA39-9F5EB24C32EE}] DsadcDitheringEnable [/cover] -->
                            <v:var name="DsadcDitheringEnable" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the availability of internal dithering functionality for the modulator."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e3f55c6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- [cover parentID={574864EA-4DDA-401c-B03A-2A6097AB1B0C}] DsadcInputGain [/cover] -->
                            <v:var name="DsadcInputGain" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines multiplication Gain factor for the analog input signal. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-54e9f55c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_INPUT_GAIN_FACTOR_1"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_INPUT_GAIN_FACTOR_1</a:v>
                                <a:v>DSADC_INPUT_GAIN_FACTOR_2</a:v>
                                <a:v>DSADC_INPUT_GAIN_FACTOR_4</a:v>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={A17AE756-65BD-4d1e-888A-7DADF652DC3F}] DsadcInputMuxActionMode [/cover] -->
                            <v:var name="DsadcInputMuxActionMode" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines the control mechanism for input multiplexer. It defines the action to be taken for the input multiplexer upon a trigger event for pin selection. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e4f55c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_INPUTMUX_PRESET_MODE"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_INPUTMUX_PRESET_MODE</a:v>
                                <a:v>DSADC_INPUTMUX_SINGLE_STEP_MODE</a:v>
                              </a:da>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_WINDOW'"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(../../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_NORMAL') and (. != 'DSADC_INPUTMUX_PRESET_MODE')"
                                       true="If DsadcTriggerMode is configured as TRIGGER_MODE_NORMAL then DsadcInputMuxActionMode should be configured as DSADC_INPUTMUX_PRESET_MODE."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={5582BC26-53FE-4775-8435-D216FFC02632}] DsadcInputMuxControlMode [/cover] -->
                            <v:var name="DsadcInputMuxControlMode" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines the condition for a trigger event to control the input multiplexer. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e5f55c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_INMUX_SOFTWARE_CONTROL"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_INMUX_SOFTWARE_CONTROL</a:v>
                                <a:v>DSADC_INMUX_TRIG_EVENT_FALLING_EDGE</a:v>
                                <a:v>DSADC_INMUX_TRIG_EVENT_RISING_EDGE</a:v>
                                <a:v>DSADC_INMUX_TRIG_EVENT_BOTH_EDGES</a:v>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../../DsadcTriggerMode = 'DSADC_TRIGGER_MODE_NORMAL') and (. != 'DSADC_INMUX_SOFTWARE_CONTROL'))"
                                      true="If DsadcTriggerMode is configured as TRIGGER_MODE_NORMAL then DsadcInputMuxControlMode should be Configured as DSADC_INMUX_SOFTWARE_CONTROL."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={D3F59BD2-7469-46c8-A89E-D0F99E981876}] DsadcInputPinSelection [/cover] -->
                            <v:var name="DsadcInputPinSelection" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines the initial/permanent setting for the input multiplexer, based on the operating mode selected. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e6f55c6ccc"/>
                              <a:da name="DEFAULT" type="XPath" expr="ecu:list(concat('Dsadc.InputPinSelection',text:split(node:fallback(../../DsadcHwChannelNum,'DSADC_CHANNEL_BMD'),'_')[3]))[1]"/>
                              <a:da name="RANGE" type="XPath" expr="ecu:list(concat('Dsadc.InputPinSelection',text:split(node:fallback(../../DsadcHwChannelNum,'DSADC_CHANNEL_BMD'),'_')[3]))"/>
                            </v:var>
                            <!-- [cover parentID={FFD2DC14-A863-4c05-9EC4-19C35E272DD0}] DsadcIntegratorResetEnable [/cover] -->
                            <v:var name="DsadcIntegratorResetEnable" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the modulator overload handling. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e7f55c6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="((../../DsadcDemodulatorConfiguration/*[1]/DsadcIntegratorTriggerMode = 'DSADC_INTR_BYPASSED') and (. = 'true'))"
                                       true="when the Integrator is bypassed the integrator reset enable can not set to true."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={330AF575-1E9E-4a77-A575-6BDBD9C83224}] DsadcNegativeInputLine [/cover] -->
                            <v:var name="DsadcNegativeInputLine" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter defines the modulator internal connection of the negative input. "/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-64e9f55c6ccc"/>
                              <a:da name="DEFAULT" value="DSADC_NEG_INPUT_PIN"/>
                              <a:da name="RANGE">
                                <a:v>DSADC_NEG_INPUT_PIN</a:v>
                                <a:v>DSADC_NEG_IN_SUPPLY_VOLT</a:v>
                                <a:v>DSADC_NEG_IN_COMMON_MODE_VOLT</a:v>
