                          <!-- [cover parentID={BEC5C7A0-4E3F-41aa-A7B1-288725D9C2DC}] DsadcCommonModeVoltConfig [/cover] -->
                          <v:ctr name="DsadcCommonModeVoltConfig" type="IDENTIFIABLE">
                            <a:a name="UUID" value="6062ca80-76f1-43eb-8fae-6b1ab59e2ccc"/>
                            <a:a name="DESC" value="EN: This container provides configuration parameter related to common mode voltage application to the inputs of the modulator channel."/>
                            <!-- [cover parentID={CCCCD913-5306-4807-AF35-D874754F99E2}] DsadcCommonModeVoltageEnable [/cover] -->
