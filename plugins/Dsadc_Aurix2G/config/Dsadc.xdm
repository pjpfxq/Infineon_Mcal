                                  <a:tst expr="(../DsadcIntegratorTriggerMode != 'DSADC_INTR_BYPASSED') and (num:i(count(./*)) = num:i(0))"
                                         true="The integrator is enabled , yet no container for DsadcIntegratorConfiguration is added."/>
                                </a:da>
                                <v:ctr name="DsadcIntegratorConfiguration" type="IDENTIFIABLE">
                                  <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  </a:a>
                                  <a:a name="UUID" value="6062ca80-76f1-43eb-8fae-6bbcd59e2ccc"/>
                                  <a:a name="DESC" value="EN: This container provides configuration parameter related to the Integrator functional block."/>
                                  <a:da name="INVALID" type="XPath">
                                    <a:tst expr="(../../DsadcIntegratorTriggerMode = 'DSADC_INTR_BYPASSED')"
                                           true="DsadcIntegratorConfiguration container is added, even though the integrator is bypassed."/>
                                  </a:da>
                                  <!-- [cover parentID={C99560E9-1A25-4f70-AC77-1BF9D2E2F02D}] DsadcDiscardCount [/cover] -->
                                  <v:var name="DsadcDiscardCount" type="INTEGER">
                                    <a:a name="DESC" value="EN: This parameter defines the number of result values to be discarded before the start of the integration cycle."/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
