                                  <a:tst expr="((../DsadcAuxCicFilterEnable != 'true') and (. != 'DSADC_AUXCIC_OSR_16'))"
                                         true="This Parameter should be configured to the Default value if the parameter DsadcAuxCicFilterEnable configured as false."/>
                                </a:da>
