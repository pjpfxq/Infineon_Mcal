                          <!-- [cover parentID={165865F0-F8A1-42d2-A5E7-E2B0B5403B23}] DsadcFilterConfiguration [/cover] -->
                          <v:ctr name="DsadcFilterConfiguration" type="IDENTIFIABLE">
                            <a:a name="UUID" value="6062ca80-76f1-43eb-8fae-6b13459e2ccc"/>
                            <a:a name="DESC" value="EN: This container provides configuration parameter related to main filter chain."/>
                            <!-- [cover parentID={48174AD9-2325-4d69-992C-5BB022D54C80}] DsadcFIR0FilterEnable [/cover] -->
                            <v:var name="DsadcFIR0FilterEnable" type="BOOLEAN">
                              <a:a name="DESC" value="EN: This parameter defines the availability of FIR0 filter in the filter chain of the DSADC channel."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f60c6ccc"/>
                              <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- [cover parentID={307BCD5D-E217-494d-8691-EBE9D5097055}] DsadcFIR1FilterEnable [/cover] -->
                            <v:var name="DsadcFIR1FilterEnable" type="BOOLEAN">
