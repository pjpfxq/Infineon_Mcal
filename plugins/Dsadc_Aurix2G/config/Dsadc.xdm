                          <!--********************************************************-->
                          <!--Dsadc Channel Resolver Rectifier Configuration - Start *-->
                          <!--********************************************************-->
                          <!-- [cover parentID={967A0333-B3CA-4f47-93BA-4D42BE67B49D}] DsadcRectificationConfiguration [/cover] -->
                          <v:lst name="DsadcRectificationConfiguration" type="MAP">
                            <a:da name="MIN" value="0"/>
                            <a:da name="MAX" value="1"/>
                            <v:ctr name="DsadcRectificationConfiguration" type="IDENTIFIABLE">
                              <a:a name="UUID" value="6062ca80-76f1-43eb-8fae-6b10c59e2ccc"/>
                              <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="DESC" value="EN: This container provides configuration parameters of the rectifier block, used for resolver support, to determine position of the motor."/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(../../DsadcDemodulatorConfiguration/*[1]/DsadcIntegratorTriggerMode = 'DSADC_INTR_BYPASSED')"
                                      true="The driver doesn't support rectification , yet container for DsadcRectificationConfiguration is added."/>
                              </a:da>
                              <!-- [cover parentID={A1E69652-28FC-4744-A59B-2B73703189DB}] DsadcRectificationEnable [/cover] -->
                              <v:var name="DsadcRectificationEnable" type="BOOLEAN">
                                <a:a name="DESC" value="EN: This parameter controls the action of the rectifier circuit on the input data."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f38c6ccc"/>
                                <a:da name="DEFAULT" value="false"/>
                              </v:var>
                              <!-- [cover parentID={781723D0-618E-4732-850B-59F3D4ED4643}] DsadcSignSignalSource [/cover] -->
                              <v:var name="DsadcSignSignalSource" type="ENUMERATION">
                                <a:a name="DESC" value="EN: This parameter selects the source of the sign signal, which is to be delayed for the purpose of rectification."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f39c6ccc"/>
                                <a:da name="DEFAULT" type="XPath" expr="ecu:list('Dsadc.SignSignalSrc')[1]"/>
                                <a:da name="RANGE" type="XPath" expr="ecu:list('Dsadc.SignSignalSrc')"/>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="((../DsadcRectificationEnable) = 'false') and (not(contains((../DsadcSignSignalSource),'SRC_0_')))"
                                         true="Since the rectifier is disabled, it is incorrect to configure parameter 'DsadcSignSignalSource'."/>
                                </a:da>
                              </v:var>
                              <!-- [cover parentID={6BFEAAE1-6936-4347-B23C-6AB29F521DA1}] DsadcSignSignalChannel [/cover] -->
                              <v:var name="DsadcSignSignalChannel" type="ENUMERATION">
                                <a:a name="DESC" value="EN: This parameter selects the DSADC channel which provides the source of the sign signal, which is to be delayed for the purpose of rectification."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f3ac6ccc"/>
                                <a:da name="DEFAULT" type="XPath" expr="ecu:list('Dsadc.SignSignalChannel')[1]"/>
                                <a:da name="RANGE" type="XPath" expr="ecu:list('Dsadc.SignSignalChannel')"/>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="(((../DsadcRectificationEnable) = 'false') and (not(contains((../DsadcSignSignalChannel),'CHANNEL_0')))) or (not((contains((../DsadcSignSignalSource),'SRC_1_SIGNRESULT_FROM_DSADC_CHANNEL'))) and (not(contains((../DsadcSignSignalChannel),'CHANNEL_0'))))"
                                         true=" Error could be because of 2 scenarios
                                              : Since the rectifier is disabled, it is incorrect to configure parameter 'DsadcSignSignalChannel'.
                                              : Since the source of the sign signal is on chip modulator, it is incorrect to configure parameter 'DsadcSignSignalChannel'."/>
                                </a:da>
                              </v:var>
                              <!-- [cover parentID={6704BE31-9F04-400a-85D2-7305D7BE24D4}] DsadcPosSignDelayValue [/cover] -->
                              <v:var name="DsadcPosSignDelayValue" type="INTEGER">
                                <a:a name="DESC" value="EN: This parameter determines the value of Sign Delay counter (number of result values), for which to generate a positive delayed sign signal."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f3bc6ccc"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:da name="INVALID" type="Range">
                                  <a:tst expr="&lt;=255"/>
                                  <a:tst expr="&gt;=0"/>
                                </a:da>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="((../DsadcRectificationEnable = 'false') and (../DsadcPosSignDelayValue != '0'))"
                                         true="Since the rectifier is disabled, it is incorrect to configure parameter 'DsadcPosSignDelayValue'."/>
                                </a:da>
                              </v:var>
                              <!-- [cover parentID={CD41B11B-B608-47a7-8843-2E06728BC447}] DsadcNegSignDelayValue [/cover] -->
                              <v:var name="DsadcNegSignDelayValue" type="INTEGER">
                                <a:a name="DESC" value="EN: This parameter determines the value of Sign Delay counter (number of result values), for which to generate a negative delayed sign signal."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="d6ea9dd9-1d6e-4d4b-b3a3-24e9f3cc6ccc"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:da name="INVALID" type="Range">
                                  <a:tst expr="&lt;=255"/>
                                  <a:tst expr="&gt;=0"/>
                                </a:da>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="(((../DsadcRectificationEnable = 'false') and (../DsadcNegSignDelayValue != '0')) or (../DsadcPosSignDelayValue) > (../DsadcNegSignDelayValue))"
                                         true="The error could be because of 2 reasons:
                                             - Since the rectifier is disabled, it is incorrect to configure parameter 'DsadcNegSignDelayValue'.
                                             - Positive Sign Delay value is set Higher than the Negative sign delay value."/>
                                </a:da>
                              </v:var>
                            </v:ctr>
                          </v:lst>
                          <!--********************************************************-->
                          <!--Dsadc Channel Resolver Rectifier Configuration - End   *-->
                          <!--********************************************************-->

