                    <a:da name="RANGE">
                      <a:v>I2C_MCAL_SUPERVISOR</a:v>
                      <a:v>I2C_MCAL_USER1</a:v>
                    </a:da>
                </v:var>
                <v:ref name="I2cSystemClock" type="REFERENCE">
                  <a:a name="DESC" value="EN: This parameter refer MCU read the System clock configured"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                  <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                  <a:a name="SCOPE" value="LOCAL"/>
                  <a:a name="UUID" value="7a12e6a7-f02f-4d95-8b8a-87a36bed8fb9"/>
