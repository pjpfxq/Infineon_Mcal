                </v:ref>
              </v:ctr>


              <v:ctr name="I2cConfigSet" type="IDENTIFIABLE">
                 <a:a name="DESC" value="EN: This container contains the Channel configuration of the I2C driver. This container is a MultipleConfigurationContainer, i.e. this container and its sub-containers exit once per configuration set."/>
                 <a:a name="UUID" value="439b823d-f723-43d7-b3d6-8e4c38733021"/>
                <v:lst name="I2cChannelConfiguration" type="MAP">
                  <a:da name="MIN" value="1"/>
                  <a:da name="MAX" type="XPath" expr="ecu:get('I2c.NoOfAvailableI2c')"/>
                    <v:ctr name="I2cChannelConfiguration" type="IDENTIFIABLE">
                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="DESC" value="EN: Configuration settings I2C Channel"/>
                      <a:a name="LONG-NAME" value="I2cHwConfiguration"/>
                      <a:a name="UUID" value="DCE:46354a81-d2f8-6a3d-9a0a-546962bdd907"/>
                          <v:var name="I2cHwUnit" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter selects the SDA and SCL lines for the peripheral."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="a64deb84-f92e-4ba5-98b0-c301a8becdd2"/>
                            <a:da name="DEFAULT" value="I2C_0"/>
                            <a:da name="RANGE">
                              <a:v>I2C_0</a:v>
                              <a:v>I2C_1</a:v>
                            </a:da>
                          </v:var>
                          <v:var name="I2cSDASelect" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter selects the SDA and SCL lines for the peripheral."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="3de123f4-f185-4e60-aa37-916b742c9224"/>
                            <a:da name="DEFAULT" type="XPath" expr="ecu:list(concat('I2c.I2C',substring-after(node:fallback(../I2cHwUnit,'_All'),'_'),'SDAPin'))[position()-1 = 0]"/>
                            <a:da name="RANGE" type="XPath" expr="ecu:list(concat('I2c.I2C',substring-after(node:fallback(../I2cHwUnit,'_All'),'_'),'SDAPin'))"/>
                          </v:var>
                          <v:var name="I2cSCLSelect" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter selects the SDA and SCL lines for the peripheral."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="ac0bdf46-0ffc-4a51-a2bf-e79bc3ed651e"/>
                            <a:da name="DEFAULT" type="XPath" expr="ecu:list(concat('I2c.I2C',substring-after(node:fallback(../I2cHwUnit,'_All'),'_'),'SCLPin'))[position()-1 = 0]"/>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="text:contains(node:value(../I2cSCLSelect),(text:split((text:split(../I2cSDASelect,'_')[1]),'SDA')))" false="I2C - The SDA and SCL pin selection should be from same group."/>
                           </a:da>
                            <a:da name="RANGE" type="XPath" expr="ecu:list(concat('I2c.I2C',substring-after(node:fallback(../I2cHwUnit,'_All'),'_'),'SCLPin'))"/>
                          </v:var>
                          <v:var name="I2cSpeed" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter defines the data transfer speed of the external device."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="a64deb82-f92e-4ba5-98b0-c301a8becdd2"/>
                            <a:da name="DEFAULT" value="STANDARD_MODE"/>
                            <a:da name="RANGE">
                              <a:v>STANDARD_MODE</a:v>
                              <a:v>FAST_MODE</a:v>
                              <a:v>HIGH_SPEED_MODE</a:v>
                            </a:da>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>
                          <v:var name="I2cAddressingMode" type="ENUMERATION">
                            <a:a name="DESC" value="EN: This parameter defines the Addressing mode(7/10 bit) required to address the slave."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="a05ca5e9-c0fd-4fed-90d9-6358b01c414c"/>
                            <a:da name="DEFAULT" value="I2C_7_BIT_ADDRESSING"/>
                            <a:da name="RANGE">
                              <a:v>I2C_7_BIT_ADDRESSING</a:v>
                              <a:v>I2C_10_BIT_ADDRESSING</a:v>
                            </a:da>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>
                          <v:var name="I2cFractionalDividerDec" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains DEC value of the fractional divider."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ab000ab6-a03e-4365-ab99-a3b38d66f182"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=2047"/>
                              <a:tst expr="&gt;=1"/>
                            </a:da>
                            <a:da name="DEFAULT" value="997"/>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>

                          <v:var name="I2cFractionalDividerInc" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains INC value of the fractional divider."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ab000ab7-a03e-4365-ab99-a3b38d66f182"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=255"/>
                              <a:tst expr="&gt;=1"/>
                            </a:da>
                            <a:da name="DEFAULT" value="2"/>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>
                          <v:var name="I2cRmc" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains I2cRmc value of the CLC1 register."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ab000ab8-a03e-4365-ab99-a3b38d66f182"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=255"/>
                              <a:tst expr="&gt;=1"/>
                            </a:da>
                            <a:da name="DEFAULT" value="1"/>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>

                          <v:var name="I2cSdaDelayStageDataHoldTime" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains SDA delay stage for data hold time."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="a64deb85-f92e-4ba5-98b0-c301a8becdd2"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=63"/>
                              <a:tst expr="&gt;=0"/>
                            </a:da>
                            <a:da name="EDITABLE" value="false"/>
                          </v:var>

                          <v:var name="I2cSclDelayStageHoldTimeStartBit" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains SCL delay stages for Hold time start(Restart) bit."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="a64deb86-f92e-4ba5-98b0-c301a8becdd2"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=7"/>
                              <a:tst expr="&gt;=0"/>
                            </a:da>
                            <a:da name="EDITABLE" value="false"/>
                          </v:var>

                          <v:var name="I2cSetFastModeSclLowPerTime" type="BOOLEAN">
                            <a:a name="DESC" value="EN: This parameter enables Standard or Fast mode SCL Low period timing."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="6ef30999-101c-4cfe-a0fb-d8b6a23527ac"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../I2cEnCfgFastModeSclLowLength = 'true'"/>
                            </a:da>
                          </v:var>

                          <v:var name="I2cEnCfgFastModeSclLowLength" type="BOOLEAN">
                            <a:a name="DESC" value="EN: This parameter enables Direct Configuration of SCL Low Period Length in Fast Mode."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="6ef30990-101c-4cfe-a0fb-d8b6a23527ac"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="(../I2cSpeed = 'FAST_MODE') or (../I2cSpeed = 'HIGH_SPEED_MODE')"/>
                            </a:da>
                          </v:var>

                          <v:var name="I2cFastModeSclLowLength" type="INTEGER">
                            <a:a name="DESC" value="EN: This parameter contains SCL Low Period Length in Fast Mode."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="a64deb87-f92e-4ba5-98b0-c301a8becdd2"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=255"/>
                              <a:tst expr="&gt;=0"/>
                            </a:da>
                            <a:da name="EDITABLE" type="XPath">
                              <a:tst expr="../I2cEnCfgFastModeSclLowLength = 'true'"/>
                            </a:da>
                          </v:var>
                          <v:var name="I2cAsyncNotification" type="BOOLEAN">
                            <a:a name="DESC" value="EN: Switches Asynchronous notification ON or OFF."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="d2f54233-0a95-4863-8c21-8c1ea554c81d"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="EDITABLE" value="true"/>
                          </v:var>
                          <v:var name="I2cPacketEndNotification" type="FUNCTION-NAME">
                            <a:a name="DESC" value="EN: This parameter is a reference to a notification function."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                               <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                               <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="999fcf11-8b4f-4719-abee-2f2d75e8614a"/>
