                        <!-- [cover parentID={21B450F5-989B-4095-A451-D237D777751C}] AdcGroup [/cover] -->
                        <v:lst name="AdcGroup" type="MAP">
                          <a:da name="MIN" value="1"/>
                          <a:da name="MAX" value="32"/>
                          <v:ctr name="AdcGroup" type="IDENTIFIABLE">
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="DESC" value="EN: This container contains the Group configuration (parameters)."/>
                            <a:a name="UUID" value="ECUC:21b450f5-989b-4095-a451-d237d777751c"/>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="text:uniq(../../../*/AdcGroup/*/@name,@name)" false="EN: ADC - AdcGroup node names should be unique across ADC HW Units"/>
                            </a:da>
                            <!-- [cover parentID={FAAD1EB6-43B4-4195-BACC-6245556ACB07}] AdcGroupAccessMode [/cover] -->
                            <v:var name="AdcGroupAccessMode" type="ENUMERATION">
                              <a:a name="DESC" value="EN: Type of access mode to group conversion results."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:faad1eb6-43b4-4195-bacc-6245556acb07"/>
                              <a:da name="DEFAULT" value="ADC_ACCESS_MODE_SINGLE"/>
                              <a:da name="RANGE">
                                <a:v>ADC_ACCESS_MODE_SINGLE</a:v>
                                <a:v>ADC_ACCESS_MODE_STREAMING</a:v>
                              </a:da>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING'"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_SW' and ../AdcGroupConversionMode = 'ADC_CONV_MODE_ONESHOT' and .='ADC_ACCESS_MODE_STREAMING'" true="EN: ADC - Invalid Configuration, Streaming mode is not valid for a One Shot SW triggered Group. "/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={25721D0A-D8A7-4ea3-96AB-49CED3030FCE}] AdcGroupConversionMode [/cover] -->
                            <v:var name="AdcGroupConversionMode" type="ENUMERATION">
                              <a:a name="DESC" value="EN: Type of conversion mode supported by the driver."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:25721d0a-d8a7-4ea3-96ab-49ced3030fce"/>
                              <a:da name="DEFAULT" value="ADC_CONV_MODE_ONESHOT"/>
                              <a:da name="RANGE">
                                <a:v>ADC_CONV_MODE_CONTINUOUS</a:v>
                                <a:v>ADC_CONV_MODE_ONESHOT</a:v>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={65E3660C-73A4-4172-8D28-4EDB71E96153}] AdcGroupId [/cover] -->
                            <v:var name="AdcGroupId" type="INTEGER">
                              <a:a name="DESC">
                                <a:v>EN: Numeric Id of the ADC channel group.  This value will be assigned to the symbolic name derived of the AdcGroup containers short name. The range of IDs for each ADC Hw Unit:
                                    ADC0: 0 to 31, ADC1: 32 to 63, ADC2: 64 to 95, ADC3: 96 to 127, ADC4: 128 to 159, ADC5: 160 to 191, ADC6: 192 to 223, ADC7: 224 to 255, ADC8: 256 to 287
                                    ADC9: 288 to 319, ADC10: 320 to 351, ADC11: 352 to 383
                                  Recommendation  : Use AUTO-CALCULATE-VALUE option for all nodes AdcGroupId</a:v>
                              </a:a>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                              <a:a name="UUID" value="ECUC:65e3660c-73a4-4172-8d28-4edb71e96153"/>
                              <a:da name="DEFAULT" type="XPath" expr="num:i(node:fallback(node:current()/../@index,'0') + (num:i(32) * text:split(node:fallback(../../../AdcHwUnitId,'HWUNIT_ADC0'),'_ADC')[2]))"/>
                              <a:da name="INVALID" type="Multi">
                                <mt:xpath expr="(.&gt;= (num:i(32) * text:split(../../../AdcHwUnitId,'_ADC')[2]) ) and (.&lt; num:i((num:i(32) * text:split(../../../AdcHwUnitId,'_ADC')[2]) + count(node:current()/../../*)))" false="EN: ADC - Value out of range: Correct Range is (HwUnitId * 32) to ((HwUnitId * 32) + N-1), where N is number of configured groups). User can use AUTO-CALCULATE-VALUE for all the groups to solve AdcGroupId errors."/>
                                <mt:xpath expr="text:uniq(../../*/AdcGroupId,.)" false="EN: ADC - Duplicated value, AdcGroupId must be unique across all AdcGroups. User can use AUTO-CALCULATE-VALUE for all the groups to solve AdcGroupId errors."/>
                                <mt:range>
                                  <mt:tst expr="&gt;=0"/>
                                  <mt:tst expr="&lt;=ecu:get('Adc.MaxGroupId')"/>
                                </mt:range>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={C3C0273D-3FBB-48ea-B64A-6C6592D6A05C}] AdcGroupPriority [/cover] -->
                            <v:lst name="AdcGroupPriority">
                              <a:da name="MIN" type="XPath" expr="node:fallback(../../../../../../AdcGeneral/AdcPriorityImplementation,'ADC_PRIORITY_NONE') != 'ADC_PRIORITY_NONE'" true="1" false="0"/>
                              <a:da name="MAX" type="XPath" expr="node:fallback(../../../../../../AdcGeneral/AdcPriorityImplementation,'false') != 'ADC_PRIORITY_NONE'" true="1" false="0"/>
                              <v:var name="AdcGroupPriority" type="INTEGER">
                                <a:a name="DESC" value="EN: Priority level of the AdcGroup."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:c3c0273d-3fbb-48ea-b64a-6c6592d6a05c"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:da name="INVALID" type="Range">
                                  <a:tst expr="&lt;=255"/>
                                  <a:tst expr="&gt;=0"/>
                                </a:da>
                              </v:var>
                            </v:lst>
                            <!-- [cover parentID={F348B1D0-AABC-46ee-A71A-02CC6E49DCA6}] AdcGroupReplacement [/cover] -->
                            <v:lst name="AdcGroupReplacement">
                              <a:da name="MAX" value="1"/>
                              <v:var name="AdcGroupReplacement" type="ENUMERATION">
                                <a:a name="DESC" value="EN: Replacement mechanism, which is used on ADC group level, if a group conversion is interrupted by a group which has a higher priority."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:f348b1d0-aabc-46ee-a71a-02cc6e49dca6"/>
                                <a:da name="DEFAULT" value="ADC_GROUP_REPL_ABORT_RESTART"/>
