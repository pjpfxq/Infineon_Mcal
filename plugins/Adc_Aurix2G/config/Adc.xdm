                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(. = 'ADC_TRIGG_SRC_HW') and (../../../../../../AdcGeneral/AdcHwTriggerApi = 'false')" true="EN: ADC - Invalid Configuration, HW trigger source is not allowed if AdcHwTriggerApi is set to false."/>
                                <a:tst expr="(. = 'ADC_TRIGG_SRC_SW') and (../../../../../../AdcGeneral/AdcEnableStartStopGroupApi = 'false')" true="EN: ADC - Invalid Configuration, SW trigger source is not allowed if AdcEnableStartStopGroupApi is set to false."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={70C09B6D-3CAC-4db1-BEE8-AAFEF6B9D2CF}] AdcHwTrigSignal [/cover] -->
                            <v:lst name="AdcHwTrigSignal">
                              <a:da name="MIN" type="XPath" expr="node:fallback(../AdcGroupTriggSrc,'ADC_TRIGG_SRC_SW') = 'ADC_TRIGG_SRC_HW'" true="1" false="0"/>
                              <a:da name="MAX" type="XPath" expr="node:fallback(../AdcGroupTriggSrc,'ADC_TRIGG_SRC_HW') = 'ADC_TRIGG_SRC_HW'" true="1" false="0"/>
                              <v:var name="AdcHwTrigSignal" type="ENUMERATION">
                                <a:a name="DESC" value="EN: Configures on which edge of the hardware trigger signal the driver should react, i.e. start the conversion (only if supported by the ADC hardware)."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:70c09b6d-3cac-4db1-bee8-aafef6b9d2cf"/>
                                <a:da name="DEFAULT" value="ADC_HW_TRIG_RISING_EDGE"/>
                                <a:da name="RANGE">
                                  <a:v>ADC_HW_TRIG_BOTH_EDGES</a:v>
                                  <a:v>ADC_HW_TRIG_FALLING_EDGE</a:v>
                                  <a:v>ADC_HW_TRIG_RISING_EDGE</a:v>
                                </a:da>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="(node:exists(../../GtmTriggerTimerConfig/*[1]) and (node:value(.) != 'ADC_HW_TRIG_RISING_EDGE')) = true()" true="EN: ADC - Invalid Configuration, When GTM is configured inside the ADC driver for HW trigger source, Rising edge should be selected"/>
                                </a:da>
                              </v:var>
                            </v:lst>
                            <!-- [cover parentID={AC85F536-5CF6-43f3-A6A7-D824EED38461}] AdcHwTrigTimer [/cover] -->
                            <v:lst name="AdcHwTrigTimer">
                              <a:da name="MAX" type="XPath" expr="node:fallback(../AdcGroupTriggSrc,'ADC_TRIGG_SRC_HW') = 'ADC_TRIGG_SRC_SW'" true="0" false="1"/>
                              <v:var name="AdcHwTrigTimer" type="INTEGER">
                                <a:a name="DESC" value="EN: Reload value of the ADC module embedded timer (only if supported by ADC hardware)."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:ac85f536-5cf6-43f3-a6a7-d824eed38461"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:da name="INVALID" type="Range">
                                  <a:tst expr="&lt;=1023"/>
                                  <a:tst expr="&gt;=0"/>
                                </a:da>
                              </v:var>
                            </v:lst>
                            <!-- [cover parentID={F43BFFDA-8D3A-4520-B424-B558FCBE752C}] AdcNotification [/cover] -->
                            <v:lst name="AdcNotification">
                              <a:da name="MIN" value="0"/>
                              <a:da name="MAX" type="XPath" expr="node:fallback(../../../../../../AdcGeneral/AdcGrpNotifCapability,'true') = 'true'" true="1" false="0"/>
                              <v:var name="AdcNotification" type="FUNCTION-NAME">
                                <a:a name="DESC" value="EN: Callback function for each group"/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:f43bffda-8d3a-4520-b424-b558fcbe752c"/>
                                <a:da name="DEFAULT" type="XPath" expr="concat('IoHwAb_AdcNotification',node:fallback(../../AdcGroupId,'0'))"/>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="((. = 'NULL')or(. = '') or (.=' '))" true="EN: ADC - Invalid Configuration, If Notification is not desired, then remove the Notification container. Value of NULL or empty field is not allowed."/>
                                </a:da>
                              </v:var>
                            </v:lst>
                            <!-- [cover parentID={7EA9390C-D8F5-44c3-BAF2-D2575EF9E9C8}] AdcStreamingBufferMode [/cover] -->
                            <v:var name="AdcStreamingBufferMode" type="ENUMERATION">
                              <a:a name="DESC" value="EN: Configure streaming buffer as &quot;linear buffer&quot; (i.e. the ADC Driver stops the conversion as soon as the stream buffer is full) or as &quot;ring buffer&quot; (wraps around if the end of the stream buffer is reached)."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:7ea9390c-d8f5-44c3-baf2-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" value="ADC_STREAM_BUFFER_LINEAR"/>
                              <a:da name="RANGE">
                                <a:v>ADC_STREAM_BUFFER_CIRCULAR</a:v>
                                <a:v>ADC_STREAM_BUFFER_LINEAR</a:v>
                              </a:da>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING'"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_SW' and ../AdcGroupConversionMode = 'ADC_CONV_MODE_ONESHOT' and ../AdcGroupAccessMode='ADC_ACCESS_MODE_SINGLE' and  .= 'ADC_STREAM_BUFFER_CIRCULAR'" true="EN: ADC - Invalid Configuration, Streaming Buffer cannot be circular for One Shot SW triggered group in Single access mode"/>
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_SW' and ../AdcGroupConversionMode = 'ADC_CONV_MODE_CONTINUOUS' and ../AdcGroupAccessMode='ADC_ACCESS_MODE_SINGLE' and  .= 'ADC_STREAM_BUFFER_LINEAR'" true="EN: ADC - Invalid Configuration, Streaming Buffer cannot be linear for Continuous SW triggered group in Single access mode"/>
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW' and ../AdcGroupAccessMode='ADC_ACCESS_MODE_SINGLE' and .= 'ADC_STREAM_BUFFER_LINEAR'" true="EN: ADC - Invalid Configuration, Streaming Buffer cannot be linear for HW triggered group in Single access mode."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={BA5E660F-1505-40bb-9B17-7CE8A5655235}] AdcStreamingNumSamples [/cover] -->
                            <v:var name="AdcStreamingNumSamples" type="INTEGER">
                              <a:a name="DESC" value="EN: Number of ADC values to be acquired per channel in streaming access mode."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:ba5e660f-1505-40bb-9b17-7ce8a5655235"/>
                              <a:da name="DEFAULT" value="1"/>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING'"/>
                              </a:da>
                              <a:da name="INVALID" type="Multi">
                                <mt:xpath expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupAccessMode = 'ADC_ACCESS_MODE_SINGLE' and (.!='1')" true="EN: ADC - Invalid configuration. For single access mode, number of streaming samples should be 1."/>
                                <mt:xpath expr="../../../../../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_INTERRUPT_MODE_RESULT_HANDLING' and
                                  ../AdcGroupAccessMode = 'ADC_ACCESS_MODE_STREAMING' and (. &lt;= '1')" true="EN: ADC - Invalid configuration. For streaming access mode, number of samples should be greater than 1."/>
                                <mt:range>
                                  <mt:tst expr="&gt;=1"/>
                                  <mt:tst expr="&lt;=255"/>
                                </mt:range>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={0132CACC-57D0-48c3-979F-787109AB31FC}] AdcHwExtTrigSelect [/cover] -->
                            <v:var name="AdcHwExtTrigSelect" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter configures the HW trigger source for the group."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:7ea9390c-d8f5-44c3-2af2-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" type="XPath" expr="normalize-space(text:split(ecu:get('Adc.TriggerSource'),',')[1])"/>
                              <a:da name="RANGE" type="XPath" expr="ecu:list('Adc.TriggerSource')"/>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW'"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="(. != 'ADC_TRIG_NONE') and (../AdcGroupTriggSrc= 'ADC_TRIGG_SRC_SW')" true="EN: ADC - Invalid Configuration, For SW triggered groups, AdcHwExtTrigSelect should be ADC_TRIG_NONE"/>
                                <a:tst expr="(. = 'ADC_TRIG_NONE') and (../AdcGroupTriggSrc= 'ADC_TRIGG_SRC_HW') and ((num:i(count(../AdcHwTrigTimer/*)) = 0) or (../AdcHwTrigTimer/*[1] = 0))" true="EN: ADC - Invalid Configuration, For HW triggered groups, either AdcHwExtTrigSelect should not be ADC_TRIG_NONE or AdcHwTrigTimer should not be 0."/>
                                <a:tst expr="(. != 'ADC_TRIG_NONE') and (num:i(count(../AdcHwTrigTimer/*)) &gt; 0) and (../AdcHwTrigTimer/*[1] != 0)" true="EN: ADC - Invalid Configuration, For HW triggered groups, AdcHwExtTrigSelect should be ADC_TRIG_NONE if AdcHwTrigTimer is not 0."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={5FF7D13C-00CB-445b-BA27-230CACB3EE3B}] AdcHwExtGateSelect [/cover] -->
                            <v:var name="AdcHwExtGateSelect" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter configures the HW gating source for the group."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:7ea9390c-d8f5-44c3-baf1-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" type="XPath" expr="normalize-space(text:split(ecu:get('Adc.GatingSource'),',')[1])"/>
                              <a:da name="RANGE" type="XPath" expr="ecu:list('Adc.GatingSource')"/>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW'"/>
                              </a:da>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr=". != 'ADC_GATE_NONE' and ../AdcGroupTriggSrc= 'ADC_TRIGG_SRC_SW'" true="EN: ADC - Invalid Configuration, SW triggered groups,AdcHwExtGateSelect should be ADC_GATE_NONE"/>
                                <a:tst expr="(../AdcHwExtTrigSelect = 'ADC_TRIG_15_GxREQTRP_GxREQGTySEL') and (not(contains (., 'CCU6')))" true="EN: ADC - Invalid Configuration, For groups triggered via the gate line, only CCU6 peripheral should be selected in AdcHwExtGateSelect. Since, other peripherals are directly connected to the trigger line of the Request Source"/>
                                <a:tst expr="(. != 'ADC_GATE_NONE') and (num:i(count(../AdcHwTrigTimer/*)) &gt; 0) and (../AdcHwTrigTimer/*[1] != 0)" true="EN: ADC - Invalid Configuration, For HW triggered groups, AdcHwExtGateSelect should be ADC_GATE_NONE if AdcHwTrigTimer is not 0."/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={C13A6414-C229-44b6-A6D6-6DF2D1405DCA}] AdcHwGateSignal [/cover] -->
                            <v:var name="AdcHwGateSignal" type="ENUMERATION">
                              <a:a name="DESC" value="EN: This parameter configures the HW gating signal level."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:8ea9390c-d8f5-44c3-baf1-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" value="ADC_GATE_LVL_HIGH"/>
                              <a:da name="RANGE">
                                <a:v>ADC_GATE_LVL_HIGH</a:v>
                                <a:v>ADC_GATE_LVL_LOW</a:v>
                              </a:da>
                              <a:da name="EDITABLE" type="XPath">
                                <a:tst expr="../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW'  and ../AdcHwExtGateSelect != 'ADC_GATE_NONE' and ../AdcHwExtTrigSelect != 'ADC_TRIG_15_GxREQTRP_GxREQGTySEL'"/>
                              </a:da>
                            </v:var>
                            <v:var name="AdcChannel0Alias" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter configures the alias channel to be used for Channel 0."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:8ea93903-d8f5-44c3-baf1-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" value="0"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&gt;=0"/>
                                <a:tst expr="&lt;=31"/>
                              </a:da>
                            </v:var>
                            <v:var name="AdcChannel1Alias" type="INTEGER">
                              <a:a name="DESC" value="EN: This parameter configures the alias channel to be used for Channel 1."/>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" value="ECUC:8ea93913-d8f5-44c3-baf1-d2575ef9e9c8"/>
                              <a:da name="DEFAULT" value="1"/>
                              <a:da name="INVALID" type="Range">
                                <a:tst expr="&gt;=0"/>
                                <a:tst expr="&lt;=31"/>
                              </a:da>
                            </v:var>
                            <!-- [cover parentID={FC412877-D8AB-4b65-96E8-0498AC13762E}] AdcGroupDefinition [/cover] -->
                            <v:lst name="AdcGroupDefinition">
                              <a:da name="MIN" value="1"/>
                              <a:da name="MAX" type="XPath" expr="contains(ecu:get('Adc.SecondaryHwUnitId'),node:fallback(../../../AdcHwUnitId,'HWUNIT_ADC8')) = 'true'" true="16" false="8"/>
                              <v:ref name="AdcGroupDefinition" type="REFERENCE">
                                <a:a name="DESC" value="EN: Assignment of AdcChannels to a AdcGroups."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="UUID" value="ECUC:fc412877-d8ab-4b65-96e8-0498ac13762e"/>
                                <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Adc/AdcConfigSet/AdcHwUnit/AdcChannel"/>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="text:split(.,'/')[5] != ../../../../@name" true="EN: ADC - The selected channel does not belong to this HW Unit. Channel must be mapped in the same AdcHwUnit"/>
                                  <a:tst expr="node:refvalid(.)" false="EN: ADC - The configured node does not exist or cannot be referenced."/>
                                  <a:tst expr="text:uniq(../../AdcGroupDefinition/*,.)" false="EN: ADC - An AdcChannel cannot be repeated in an AdcGroup."/>
                                </a:da>
                              </v:ref>
                            </v:lst>
                            <v:lst name="AdcResRegDefinition">
                              <a:da name="MIN" value="1"/>
                              <a:da name="MAX" value="16"/>
                              <a:da name="INVALID" type="XPath">
                                <a:tst expr="num:i(count(../AdcGroupDefinition/*)) != num:i(count(./*))" true="EN: ADC - The Number of result registers configured should be equal to the number of channels present in the Group"/>
                              </a:da>
                              <!-- [cover parentID={5FDB8167-9ADE-4a63-B27A-07CCFDFE164D}] AdcResRegDefinition [/cover] -->
                              <v:var name="AdcResRegDefinition" type="INTEGER">
                                <a:a name="DESC" value="EN: This parameter defines the result register used for storing the result of this particular ADC conversion. This value of this parameter should be in descending order for the nodes of AdcGroupDefinition."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                  <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:ea7f8f6d-5d98-4bc4-321e-dc02c4c233fc"/>
                                <a:da name="DEFAULT" type="XPath" expr="num:i( node:fallback(node:current()/@index,'0') + node:fallback(../../AdcResRegDefinition/*[1],'0'))"/>
                                <a:da name="INVALID" type="Multi">
                                  <!-- [cover parentID={D54392C5-B505-4156-BD08-70637E48B7C2}] AdcResRegDefinition is implemented to be contiguous [/cover] -->
                                  <mt:xpath expr="(.) != num:i(node:current()/@index + ../../AdcResRegDefinition/*[1])" true="EN: ADC - Result Registers should be configured in contiguous-ascending order for the channels."/>
                                  <mt:range>
                                    <mt:tst expr="&gt;=0"/>
                                    <mt:tst expr="&lt;=15"/>
                                  </mt:range>
                                </a:da>
                              </v:var>
                            </v:lst>
                            <!-- [cover parentID={C7B3143C-3252-4ee9-AA65-BC3BC026D831}] EruTriggerConfig [/cover] -->
                            <v:lst name="EruTriggerConfig" type="MAP">
                              <a:da name="MIN" type="XPath" expr="contains(node:fallback(../AdcHwExtTrigSelect,'NONE'),'ERUIOUTx')" true="1" false="0"/>
                              <a:da name="MAX" type="XPath" expr="contains(node:fallback(../AdcHwExtTrigSelect,'ERUIOUTx'),'ERUIOUTx')" true="1" false="0"/>
                              <v:ctr name="EruTriggerConfig" type="IDENTIFIABLE">
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                     type="IMPLEMENTATIONCONFIGCLASS">
                                  <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="DESC">
                                  <a:v>EN: This container configures the parameters for ERU triggering </a:v>
                                </a:a>
                                <a:a name="UUID" value="83112075-2689-470a-b302-a6fe4b0bd42d"/>
                                <!-- [cover parentID={1F10CBB1-810B-4732-95FC-178139DCAA17}] AdcEruErsRef [/cover] -->
                                <v:ref name="AdcEruErsRef" type="REFERENCE">
                                  <a:a name="DESC" value="EN: This parameter is a reference to the ERU container in the MCU. It lists down all the ERU-ERS channels available."/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
