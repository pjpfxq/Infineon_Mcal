                  <!-- [cover parentID={DA0AAFF6-3ED6-4a15-824E-DC32F3A8BD93}] AdcGeneral [/cover] -->
                  <v:ctr name="AdcGeneral" type="IDENTIFIABLE">
                    <a:a name="DESC" value="EN: General configuration (parameters) of the ADC Driver software module."/>
                    <a:a name="UUID" value="ECUC:da0aaff6-3ed6-4a15-824e-dc32f3a8bd93"/>
                    <!-- [cover parentID={16C0E8FD-6160-4ffb-B5A0-0FA22757DBAE}] AdcDeInitApi [/cover] -->
                    <v:var name="AdcDeInitApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the service Adc_DeInit() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:16c0e8fd-6160-4ffb-b5a0-0fa22757dbae"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={DB321B92-621A-4a62-8778-09B8845D8F2C}] AdcDevErrorDetect [/cover] -->
                    <v:var name="AdcDevErrorDetect" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Switches the Default Error Tracer (Det) detection and notification ON or OFF."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:db321b92-621a-4a62-8778-09b8845d8f2c"/>
                      <a:da name="DEFAULT" value="true"/>
                    </v:var>
                    <v:var name="AdcSafetyEnable" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Enable/Disable the Safety check and all associated notifications."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:412da385-6bf4-4819-11e4-8ec223fb5b17"/>
                      <a:da name="DEFAULT" value="true"/>
                    </v:var>
                    <v:var name="AdcMultiCoreErrorDetect" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the Multicore error detection feature from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:dbfab416-7231-4c5e-a7a2-2580d8484ece"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="(../AdcDevErrorDetect = 'false') and (. = 'true')" true="EN: ADC - Invalid Configuration, Multi-Core Error Detect must be enabled only when Dev Error Detect is enabled "/>
                      </a:da>
                      <a:da name="DEFAULT" value="true"/>
                    </v:var>
                    <v:var name="AdcInitCheckApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the service Adc_InitCheck() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:dbfab416-7231-4c9e-b7a2-2580d8484ecf"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={36543FC4-CDC1-453b-9706-844D76E6FA30}] AdcEnableLimitCheck [/cover] -->
                    <v:var name="AdcEnableLimitCheck" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Enables or disables limit checking feature in the ADC driver."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:36543fc4-cdc1-453b-9706-844d76e6fa30"/>
                      <a:da name="DEFAULT" value="false"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr=". = 'true' and ../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_DMA_MODE_RESULT_HANDLING'" true="EN: ADC - Invalid Configuration, AdcEnableLimitCheck should be false, when AdcResultHandlingImplementation is ADC_DMA_MODE_RESULT_HANDLING (DMA mode of result handling is enabled) "/>
                      </a:da>
                    </v:var>
                    <!-- [cover parentID={CF5318B5-011B-4128-8393-02DD46FE52B5}] AdcEnableQueuing [/cover] -->
                    <v:var name="AdcEnableQueuing" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Determines, if the queuing mechanism is active in case of priority mechanism disabled."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:cf5318b5-011b-4128-8393-02dd46fe52b5"/>
                      <a:da name="DEFAULT" value="false"/>
                      <a:da name="EDITABLE" type="XPath">
                        <a:tst expr="../AdcPriorityImplementation = 'ADC_PRIORITY_NONE' and ../AdcResultHandlingImplementation != 'ADC_DMA_MODE_RESULT_HANDLING'"/>
                      </a:da>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr=". = 'true' and ../../AdcGeneral/AdcEnableStartStopGroupApi = 'false'" true="EN: ADC - Queuing mode should be enabled only when AdcEnableStartStopGroupApi is true (Only SW triggered groups can be queued) "/>
                        <a:tst expr=". = 'true' and ../../AdcGeneral/AdcPriorityImplementation != 'ADC_PRIORITY_NONE'" true="EN: ADC - Invalid Configuration, AdcEnableQueuing should be false, when AdcPriorityImplementation is not ADC_PRIORITY_NONE (Priority mechanism is enabled) "/>
                        <a:tst expr=". = 'true' and ../../AdcGeneral/AdcResultHandlingImplementation = 'ADC_DMA_MODE_RESULT_HANDLING'" true="EN: ADC - Invalid Configuration, AdcEnableQueuing should be false, when AdcResultHandlingImplementation is ADC_DMA_MODE_RESULT_HANDLING (DMA mode of result handling is enabled) "/>
                      </a:da>
                    </v:var>
                    <!-- [cover parentID={94B12C2C-4231-424c-90C9-A55EE72663FE}] AdcEnableStartStopGroupApi [/cover] -->
                    <v:var name="AdcEnableStartStopGroupApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the services Adc_StartGroupConversion() and Adc_StopGroupConversion() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:94b12c2c-4231-424c-90c9-a55ee72663fe"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={B2CFCB7B-0A84-4c50-BEBD-32EE405E4300}] AdcGrpNotifCapability [/cover] -->
                    <v:var name="AdcGrpNotifCapability" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Determines, if the group notification mechanism (the functions to enable and disable the notifications) is available at runtime."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:b2cfcb7b-0a84-4c50-bebd-32ee405e4300"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={E5C85E73-38E2-47ae-85E9-C39D1B361249}] AdcHwTriggerApi [/cover] -->
                    <v:var name="AdcHwTriggerApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the services Adc_EnableHardwareTrigger() and Adc_DisableHardwareTrigger() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:e5c85e73-38e2-47ae-81e9-c39d1b361249"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={6FE4295D-02B9-4c34-B9F2-337C2DF6BD58}] AdcReadGroupApi [/cover] -->
                    <v:var name="AdcReadGroupApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the service Adc_ReadGroup() and from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:6fe4295d-02b9-4c34-b9f2-337c2df6bd58"/>
                      <a:da name="DEFAULT" value="false"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr=". = 'true' and ../AdcResultHandlingImplementation = 'ADC_DMA_MODE_RESULT_HANDLING'" true="EN: ADC - Invalid Configuration, AdcReadGroupApi should be false, when AdcResultHandlingImplementation is ADC_DMA_MODE_RESULT_HANDLING (DMA mode for result transfer is enabled)"/>
                      </a:da>
                    </v:var>
                    <!-- [cover parentID={8EA4CB56-CF5E-4afc-95CE-CEBAC08AC0E6}] AdcVersionInfoApi [/cover] -->
                    <v:var name="AdcVersionInfoApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the service Adc_GetVersionInfo() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:8ea4cb56-cf5e-4afc-95ce-cebac08ac0e6"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={28EC2427-0DF6-45d5-9FA2-DE7C8E8A03DD}] AdcStartupCalibApi [/cover] -->
                    <v:var name="AdcStartupCalibApi" type="BOOLEAN">
                      <a:a name="DESC" value="EN: Adds / removes the service Adc_GetStartupCalStatus() and Adc_TriggerStartupCal() from the code."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:8ea42236-cf5e-4afc-95ce-cebac08ac0e6"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={AFC13A85-D2B3-49a1-ABEF-E07C241698E3}] AdcTriggerOneConversionEnable [/cover] -->
                    <v:var name="AdcTriggerOneConversionEnable" type="BOOLEAN">
                      <a:a name="DESC" value="EN: This parameter enables the execution of one dummy conversion for each configured HW unit, before start-up calibration is triggered in the API Adc_TriggerStartupCal(). Refer Errata ADC_TC.083 "/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:8ea42236-cf5e-4afc-11ce-c1bac08ac0e6"/>
                      <a:da name="DEFAULT" value="false"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr=". = 'true' and ../AdcStartupCalibApi != 'true'" true="EN: ADC - Invalid Configuration, AdcTriggerOneConversionEnable should be true only when AdcStartupCalibApi is set to true"/>
                      </a:da>
                      <a:da name="EDITABLE" type="XPath">
                        <a:tst expr="ecu:get('Adc.EnableErrataTC083') = num:i(1)"/>
                      </a:da>
                    </v:var>
                    <!-- [cover parentID={3B4EAF8C-ECFB-4c79-A32E-8FDB167E5216}] AdcSyncConvEnable [/cover] -->
                    <v:var name="AdcSyncConvEnable" type="BOOLEAN">
                      <a:a name="DESC" value="EN:  Enables the synchronous conversions across ADC HW groups."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:8ea42236-cf5e-4afc-15ce-cebac08ac0e6"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="(../AdcResultHandlingImplementation = 'ADC_DMA_MODE_RESULT_HANDLING') and (. = 'true')" true="EN: ADC - Invalid Configuration, AdcSyncConvEnable can only be true when AdcResultHandlingImplementation is equal to 'ADC_INTERRUPT_MODE_RESULT_HANDLING "/>
                      </a:da>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={C44C41AA-BFF4-460d-980A-79BDE5BAD6C5}] AdcLowPowerStatesSupport [/cover] -->
                    <v:lst name="AdcLowPowerStatesSupport">
                      <a:da name="MAX" value="1"/>
                      <v:var name="AdcLowPowerStatesSupport" type="BOOLEAN">
                        <a:a name="DESC" value="EN: Adds / removes all power state management related APIs (ADC_SetPowerState, ADC_GetCurrentPowerState, ADC_GetTargetPowerState, ADC_PreparePowerState), indicating if the HW offers low power state management."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:c44c41aa-bff4-460d-980a-79bde5bad6c5"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                    </v:lst>
                    <!-- [cover parentID={01A8D9C1-3BFD-4b88-8AB6-7D5BEBCA802C}] AdcPowerStateAsynchTransitionMode [/cover] -->
                    <v:lst name="AdcPowerStateAsynchTransitionMode">
                      <a:da name="MIN" value="0"/>
                      <a:da name="MAX" value="1"/>
                      <v:var name="AdcPowerStateAsynchTransitionMode" type="BOOLEAN">
                        <a:a name="DESC" value="EN: Enables / disables support of the ADCDriver to the asynchronous power state transition."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:01a8d9c1-3bfd-4b88-8ab6-7d5bebca802c"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                      <a:da name="EDITABLE" value="false"/>
                    </v:lst>
                    <!-- [cover parentID={7E0E69E5-6004-4264-A9B3-F0559F2409D6}] AdcPriorityImplementation [/cover] -->
                    <v:var name="AdcPriorityImplementation" type="ENUMERATION">
                      <a:a name="DESC" value="EN: Determines whether a priority mechanism is available for prioritization of the conversion requests and if available, the type of prioritization mechanism. The selection applies for groups with trigger source software and trigger source hardware. Two"/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:7e0e69e5-6004-4264-a9b3-f0559f2409d6"/>
                      <a:da name="DEFAULT" value="ADC_PRIORITY_NONE"/>
                      <a:da name="RANGE">
                        <a:v>ADC_PRIORITY_NONE</a:v>
                        <a:v>ADC_PRIORITY_HW</a:v>
                        <a:v>ADC_PRIORITY_HW_SW</a:v>
                      </a:da>
                    </v:var>
                    <!-- [cover parentID={ED36BC27-E8E1-4a6e-9255-B1607EEFF648}] AdcResultHandlingImplementation [/cover] -->
                    <v:var name="AdcResultHandlingImplementation" type="ENUMERATION">
                      <a:a name="DESC" value="EN: Determines the result handling mode for the ADC driver.ADC_INTERRUPT_MODE_RESULT_HANDLING: Conversion results are transferred in the Request Source ISR (AUTOSAR Mode).
                      ADC_DMA_MODE_RESULT_HANDLING: Conversion results are transferred using a DMA channel. ADC_POLLING_MODE_RESULT_HANDLING: Conversion results are transferred using synchronous APIs."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:7e0e69e5-6004-1956-22b3-f0559f2409d6"/>
                      <a:da name="DEFAULT" value="ADC_INTERRUPT_MODE_RESULT_HANDLING"/>
                      <a:da name="RANGE">
                        <a:v>ADC_INTERRUPT_MODE_RESULT_HANDLING</a:v>
                        <a:v>ADC_DMA_MODE_RESULT_HANDLING</a:v>
                      </a:da>
                      <a:da name="INVALID" type="XPath">
