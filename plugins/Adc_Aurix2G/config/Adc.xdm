<datamodel version="5.0" xmlns="http://www.tresos.de/_projects/DataModel2/12/root.xsd" xmlns:a="http://www.tresos.de/_projects/DataModel2/12/attribute.xsd" xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd" xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">
  <d:ctr type="AUTOSAR" factory="autosar" xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" xmlns:cd="http://www.tresos.de/_projects/DataModel2/08/customdata.xsd" xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" xmlns:variant="http://www.tresos.de/_projects/DataModel2/11/variant.xsd">
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="AURIX2G" type="AR-PACKAGE">
        <a:a name="UUID" value="ECUC:AUTOSAR"/>
        <d:lst type="TOP-LEVEL-PACKAGES">
          <d:ctr name="EcucDefs" type="AR-PACKAGE">
            <a:a name="UUID" value="ECUC:ECUCDEFS"/>
            <d:lst type="ELEMENTS">
             <!-- [cover parentID={C03229FE-4DCA-445e-A47C-1AE11E6C1832}] Adc Module [/cover] -->
              <d:chc name="Adc" type="AR-ELEMENT" value="MODULE-DEF">
                <v:ctr type="MODULE-DEF">
                  <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                    <ad:ADMIN-DATA>
                      <ad:DOC-REVISIONS>
                        <ad:DOC-REVISION>
                          <ad:REVISION-LABEL>4.2.2</ad:REVISION-LABEL>
                          <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                          <ad:DATE>2014-10-31</ad:DATE>
                        </ad:DOC-REVISION>
                      </ad:DOC-REVISIONS>
                    </ad:ADMIN-DATA>
                  </a:a>
                  <a:a name="DESC" value="EN: Configuration of the Adc (Analog Digital Conversion) module."/>
                  <a:a name="LOWER-MULTIPLICITY" value="0"/>
                  <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                  <!-- [cover parentID={28EA7D71-A236-4ddd-AB32-C011D40DEC3C}]Autosar 422 [/cover] -->
                  <a:a name="RELEASE" value="asc:4.2.2"/>
                  <d:ref type="REFINED_MODULE_DEF" value="ASPath:/AUTOSAR/EcucDefs/Adc"/>
                  <a:a name="UPPER-MULTIPLICITY" value="1"/>
                  <a:a name="UUID" value="ECUC:c03229fe-4dca-445e-a47c-1ae11e6c1832"/>
                  <!-- [cover parentID={31C6ACDD-AF3D-4d6f-8EBD-FA60548C0A05}] Config Variant [/cover] -->
                  <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Config Variant"/>
                    <a:da name="DEFAULT" value="VariantPostBuild"/>
                    <a:da name="RANGE">
                      <a:v>VariantPostBuild</a:v>
                    </a:da>
                  </v:var>
                  <!-- [cover parentID={FC6E0617-8C73-4b71-B09E-DFB10B76E50D}] AdcConfigSet [/cover] -->
                  <v:ctr name="AdcConfigSet" type="IDENTIFIABLE">
                    <a:a name="DESC" value="EN: This container contains the configuration parameters and sub containers of the AUTOSAR Adc module."/>
                    <a:a name="UUID" value="ECUC:fc6e0617-8c73-4b71-b09e-dfb10b76e50d"/>
                    <v:var name="AdcSyncClockDisable" type="BOOLEAN">
                      <a:a name="DESC" value="EN:This parameter determines whether the analog clock is generated in synchronized/unsynchronized mode. The configuration of the Converter  Control (CONVCTRL) clock is done by the MCU driver"/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" value="ECUC:e5c85e73-38e2-47ae-85e9-c39d1b361249"/>
                      <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    <!-- [cover parentID={DEBB64B9-A741-42c8-91EE-04CDB4E53581}] AdcSystemClock [/cover] -->
                    <v:ref name="AdcSystemClock" type="REFERENCE">
                      <a:a name="DESC" value="EN: This parameter refers to the system clock configured by the MCU driver. With this system clock, the reload value corresponding to the configured timer is calculated for the timer triggered ADC groups."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="UUID" value="ECUC:66c8e900-6197-56a3-8125-a695aa21788e"/>
                      <a:da name="REF">
                      <a:v>ASPathDataOfSchema:/AURIX2G/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePointConfig</a:v>
                      </a:da>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="node:refvalid(.)" false="EN: ADC - The configured node does not exist or may not be referenced."/>
                      </a:da>
                    </v:ref>
                    <!-- [cover parentID={4C7136AD-B912-4527-9C5C-028F69EBE330}] AdcGlobalInputClass [/cover] -->
                    <v:lst name="AdcGlobalInputClass" type="MAP">
                      <a:da name="MIN" value="2"/>
                      <a:da name="MAX" value="2"/>
                      <v:ctr name="AdcGlobalInputClass" type="IDENTIFIABLE">
                        <a:a name="DESC" value="EN: This container defines the Global Input Class for A-D conversion"/>
                        <a:a name="UUID" value="DCE:dcc39b88-cc3b-4358-ba87-30120722c6c3"/>
                        <!-- [cover parentID={01446983-AC58-4b83-AF96-A7CEA9D9DC12}] AdcChSampleTime [/cover] -->
                        <v:var name="AdcChSampleTime" type="INTEGER">
                          <a:a name="DESC" value="EN: This parameter defines the number of additional clock cycles to be added to the minimum sample phase of 2 sample clock cycles."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:ea7f906d-5d18-4bc4-808e-dc02c4c233ac"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=31"/>
                            <a:tst expr="&gt;=0"/>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={66A9CED0-A1F2-4f2d-9AD2-62F6864D777C}] AdcChPreChargeClkCycles [/cover] -->
                        <v:var name="AdcChPreChargeClkCycles" type="ENUMERATION">
                          <a:a name="DESC" value="EN: This parameter defines the number of analog input precharge clock cycles for standard conversions."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7e0e69e5-6224-4364-a9b3-f0559f2409d6"/>
                          <a:da name="DEFAULT" value="ADC_INPUT_PRECHARGE_CYCLES_0"/>
                          <a:da name="RANGE">
                            <a:v>ADC_INPUT_PRECHARGE_CYCLES_0</a:v>
                            <a:v>ADC_INPUT_PRECHARGE_CYCLES_8</a:v>
                            <a:v>ADC_INPUT_PRECHARGE_CYCLES_16</a:v>
                            <a:v>ADC_INPUT_PRECHARGE_CYCLES_32</a:v>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={EFCE800F-0501-4fd9-97AC-97E7666DB472}] AdcChConvMode [/cover] -->
                        <v:var name="AdcChConvMode" type="ENUMERATION">
                          <a:a name="DESC" value="EN: This parameter defines the noise reduction level for Standard Conversions."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7e0e69e5-6004-5564-a9b3-f0559f2409d6"/>
                          <a:da name="DEFAULT" value="ADC_NOISE_REDUCTION_STEPS_0"/>
                          <a:da name="RANGE">
                            <a:v>ADC_NOISE_REDUCTION_STEPS_0</a:v>
                            <a:v>ADC_NOISE_REDUCTION_STEPS_1</a:v>
                            <a:v>ADC_NOISE_REDUCTION_STEPS_3</a:v>
                            <a:v>ADC_NOISE_REDUCTION_STEPS_7</a:v>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={F966A56C-2016-4b6a-8789-FB9D1FED2918}] AdcChSESPSEnable [/cover] -->
                        <v:var name="AdcChSESPSEnable" type="BOOLEAN">
                          <a:a name="DESC" value="EN: This parameter defines whether Spread Early Sample Point for Standard Conversions is enabled/disabled."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:6fe4295d-02b9-4f34-b9f2-337c2df6bd58"/>
                          <a:da name="DEFAULT" value="false"/>
                        </v:var>
                      </v:ctr>
                    </v:lst>
                    <!-- [cover parentID={C67E7C58-3DAF-455b-A213-95AE94B248D8}] AdcHwUnit [/cover] -->
                    <v:lst name="AdcHwUnit" type="MAP">
                      <a:da name="MIN" value="1"/>
                      <a:da name="MAX" type="XPath" expr="ecu:get('Adc.MaxHwUnits')"/>
                      <v:ctr name="AdcHwUnit" type="IDENTIFIABLE">
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                           <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="DESC" value="EN: This container contains the Driver configuration (parameters) depending on grouping of channels"/>
                        <a:a name="UUID" value="ECUC:c67e7c58-3daf-455b-a213-95ae94b248d8"/>
                        <!-- [cover parentID={08F7956C-D858-438f-9504-12DF1BA10E73}] AdcClockSource [/cover] -->
                        <v:lst name="AdcClockSource">
                          <a:da name="MIN" value="0"/>
                          <a:da name="MAX" value="1"/>
                          <v:var name="AdcClockSource" type="ENUMERATION">
                            <a:a name="DESC" value="EN: The ADC module specific clock input for the ADC. This parameter is not used for AURIX 2G. The clock source for the complete EVADC IP can be selected through node Adc/AdcSystemClock "/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:da name="DEFAULT" value="ADC_SYSTEM_CLK"/>
                            <a:da name="RANGE" value="ADC_SYSTEM_CLK"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" value="ECUC:08f7956c-d858-438f-9504-12df1ba10e73"/>
                          </v:var>
                        </v:lst>
                        <!-- [cover parentID={7AA61340-E385-4037-B899-40CE3E595755}] AdcHwUnitId [/cover] -->
                        <v:var name="AdcHwUnitId" type="ENUMERATION">
                          <a:a name="DESC" value="EN: Description: Numeric ID of the HW Unit. This symbolic name allows accessing Hw Unit data. Enumeration literals are defined vendor specific."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7aa61340-e385-4037-b899-40ce3e595755"/>
                          <a:da name="DEFAULT" type="XPath" expr="normalize-space(text:split(ecu:get('Adc.HwUnitId'),',')[1])"/>
                          <a:da name="RANGE" type="XPath" expr="ecu:list('Adc.HwUnitId')"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="text:uniq(../../*/AdcHwUnitId,.)" false="EN: ADC - ADC HW Unit ID should be unique for each configured ADC kernel."/>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={4938BF6C-E8F8-4127-B13A-3D681D9C8F33}] AdcIdlePrechargeEnable [/cover] -->
                        <v:var name="AdcIdlePrechargeEnable" type="BOOLEAN">
                          <a:a name="DESC" value="EN: This parameter determines whether precharge of the sampling capacitor is enabled or not."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:6fe4295d-02b9-4f64-b9f2-33732df6bd58"/>
                          <a:da name="DEFAULT" value="false"/>
                        </v:var>
                        <!-- [cover parentID={8DB831F0-4CF8-4ab3-ADBB-DD0BEE4EC312}] AdcInputBufferEnable [/cover] -->
                        <v:var name="AdcInputBufferEnable" type="BOOLEAN">
                          <a:a name="DESC" value="EN: This parameter determines whether input buffering is enabled or not. If enabled buffering time is selected via AIPS(AdcChPreChargeClkCycles)."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:6fe4295d-02b5-4f64-b9f2-33732df6bd58"/>
                          <a:da name="DEFAULT" value="false"/>
                        </v:var>
                        <!-- [cover parentID={2E70570C-AFAA-44cd-B997-1A297147BCF8}] AdcPrechargeReference [/cover] -->
                        <v:var name="AdcPrechargeReference" type="ENUMERATION">
                          <a:a name="DESC" value="EN: This parameter determines whether VDDM/VSSM is used for pre-charging or not."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7e0e69e5-1104-5164-a9b3-f0559f2409d6"/>
                          <a:da name="DEFAULT" value="ADC_VDD_VSM_USED"/>
                          <a:da name="RANGE">
                            <a:v>ADC_VDD_VSM_USED</a:v>
                            <a:v>ADC_VDD_VSM_NOT_USED</a:v>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={AFDECFBB-6E04-4485-A3E5-E4745C71A7D1}] AdcReferencePrechargePhases [/cover] -->
                        <v:var name="AdcReferencePrechargePhases" type="ENUMERATION">
                          <a:a name="DESC" value="EN: This parameter determines the precharge clock phases for the reference input."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7e0e69d5-1104-5164-a9b3-f0559f2409d6"/>
                          <a:da name="DEFAULT" value="ADC_PRECHARGE_PHASE_1"/>
                          <a:da name="RANGE">
                            <a:v>ADC_PRECHARGE_PHASE_1</a:v>
                            <a:v>ADC_PRECHARGE_PHASE_2</a:v>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={315B8058-7B91-4981-9C94-7577D290C32A}] AdcCalibrationSampleTime [/cover] -->
                        <v:var name="AdcCalibrationSampleTime" type="ENUMERATION">
                          <a:a name="DESC" value="EN: This parameter determines the calibration sample time."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:7e0e69d4-2104-5164-a9b3-f0559f2409d6"/>
                          <a:da name="DEFAULT" value="ADC_CAL_TIME_2_TIMES_TADCI"/>
                          <a:da name="RANGE">
                            <a:v>ADC_CAL_TIME_2_TIMES_TADCI</a:v>
                            <a:v>ADC_CAL_TIME_4_TIMES_TADCI</a:v>
                            <a:v>ADC_CAL_TIME_6_TIMES_TADCI</a:v>
                            <a:v>ADC_CAL_TIME_8_TIMES_TADCI</a:v>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={0CDE93C2-2E25-4728-A556-C46638FF4317}] AdcPostCalibrationDisable [/cover] -->
                        <v:var name="AdcPostCalibrationDisable" type="BOOLEAN">
                          <a:a name="DESC" value="EN: This parameter determines whether the post calibration is enabled or disabled."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:6fe42a5d-02b5-4f64-a9f2-33732df6bd58"/>
                          <a:da name="DEFAULT" value="false"/>
                        </v:var>
                        <!-- [cover parentID={69A2F0A4-D29C-41fd-80F7-25D61ED5472D}] AdcAnalogClockSyncDelay [/cover] -->
                        <v:var name="AdcAnalogClockSyncDelay" type="INTEGER">
                          <a:a name="DESC" value="EN: This parameter determines analog clock synchronization delay."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:482b876b-9787-4e46-875a-559b7a1427f2"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:xpath expr=". &gt; (../AdcPrescale/*[1] - num:i(1)) and (../../../AdcSyncClockDisable = 'false')" true="EN: ADC - AdcAnalogClockSyncDelay should be less than equal to (AdcPrescale - 1), when Sychronised clocks are used"/>
                            <mt:range>
                              <mt:tst expr="&gt;=0"/>
                              <mt:tst expr="&lt;=7"/>
                            </mt:range>
                          </a:da>
                        </v:var>
                        <v:var name="AdcSampleSyncEnable" type="BOOLEAN">
                          <a:a name="DESC" value="EN: This parameter determines whether the Adc Sample Time Synchronization is enabled or disabled."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
