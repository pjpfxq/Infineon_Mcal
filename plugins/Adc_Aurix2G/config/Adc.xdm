                              <mt:tst expr="&lt;=3"/>
                            </mt:range>
                          </a:da>
                        </v:var>
                        <!-- [cover parentID={13CDC2FE-CF3C-4a68-93CB-A3676A293FD3}] AdcPowerStateReadyCbkRef [/cover] -->
                        <v:var name="AdcPowerStateReadyCbkRef" type="FUNCTION-NAME">
                          <a:a name="DESC" value="EN: Each instance of this parameter contains a reference to a power mode callback defined in a CDD or IoHwAbs component."/>
