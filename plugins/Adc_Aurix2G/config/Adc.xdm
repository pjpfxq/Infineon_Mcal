                      <a:a name="UUID" value="ECUC:ea7f8f6d-5d98-4bc4-808e-dc02c4c233fc"/>
                      <a:da name="DEFAULT" value="6000"/>
                      <a:da name="INVALID" type="Multi">
                        <mt:xpath expr="( (../AdcPriorityImplementation != 'ADC_PRIORITY_NONE') or ((../AdcPriorityImplementation = 'ADC_PRIORITY_NONE') and (../AdcEnableQueuing = 'true')) ) and ((. &lt; 16) or (. &gt; 16962))" true="EN: ADC - Invalid Configuration, In ADC_PRIORITY_HW_SW and ADC_PRIORITY_HW  and ADC_PRIORITY_NONE with Queuing enabled, the entered value should be from 16 to 16962."/>
                        <mt:xpath expr="((../AdcPriorityImplementation = 'ADC_PRIORITY_NONE') and (../AdcEnableQueuing = 'false')) and ((. &lt; 0) or (. &gt; 16962))" true="EN: ADC - Invalid Configuration, In ADC_PRIORITY_NONE with Queuing disabled, the entered value should be be from 0 to 16962."/>
                        <mt:range>
                          <mt:tst expr="&gt;=0"/>
                          <mt:tst expr="&lt;=16962"/>
                        </mt:range>
                      </a:da>
                    </v:var>
                    <v:var name="AdcSleepMode" type="ENUMERATION">
