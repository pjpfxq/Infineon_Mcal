<datamodel version="5.0" 
           xmlns="http://www.tresos.de/_projects/DataModel2/12/root.xsd" 
           xmlns:a="http://www.tresos.de/_projects/DataModel2/12/attribute.xsd" 
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd" 
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">

  <d:ctr type="AUTOSAR" factory="autosar" 
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:cd="http://www.tresos.de/_projects/DataModel2/08/customdata.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd"  
         xmlns:variant="http://www.tresos.de/_projects/DataModel2/11/variant.xsd">
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="AURIX2G" type="AR-PACKAGE">
        <a:a name="UUID" value="ECUC:AUTOSAR"/>
        <d:lst type="TOP-LEVEL-PACKAGES">
          <d:ctr name="EcucDefs" type="AR-PACKAGE">
            <a:a name="UUID" value="ECUC:ECUCDEFS"/>
                <d:lst type="ELEMENTS">
                    <d:chc name="Dma" type="AR-ELEMENT" value="MODULE-DEF">
                        <!-- [cover parentID={B70E4137-239D-4873-A63A-7E9D812EA8B2}]Dma [/cover] -->
                        <v:ctr type="MODULE-DEF">
                            <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                                <ad:ADMIN-DATA>
                                <ad:DOC-REVISIONS>
                                    <ad:DOC-REVISION>
                                        <ad:REVISION-LABEL>4.2.2</ad:REVISION-LABEL>
                                        <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                        <ad:DATE>2017-10-31</ad:DATE>
                                    </ad:DOC-REVISION>
                                </ad:DOC-REVISIONS>
                                </ad:ADMIN-DATA>
                            </a:a>
                            <a:a name="DESC" value="EN: Configuration of the Dma (Direct Memory Access) module."/>
                            <a:a name="LOWER-MULTIPLICITY" value="0"/>
                            <a:a name="UPPER-MULTIPLICITY" value="1"/>
                            <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                            <!-- [cover parentID={BD607180-6DDB-4175-9E75-6DCCAEBFBD5C}]Autosar 422 [/cover] -->
                            <!-- [cover parentID={5E53D0E5-6EF6-4229-92DC-E934D72F2446}]Autosar 422 [/cover] -->
                            <a:a name="RELEASE" value="asc:4.2.2"/>
                            <a:a name="UUID" value="ECUC:c03229fe-3dca-435e-a47c-1ae11e6c1832"/>
                            <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="LABEL" value="Config Variant"/>
                                <a:da name="DEFAULT" value="VariantPostBuild"/>
                                <a:da name="RANGE">
                                    <a:v>VariantPostBuild</a:v>
                                </a:da>
                            </v:var>
                            <!--********************************************************-->
                            <!--***************** DMA General Container ****************-->
                            <!--********************************************************-->
                            <!-- [cover parentID={76622332-BF2D-4d2b-AC0C-20FA9A049E07}] DmaGeneral [/cover] -->
                            <v:ctr name="DmaGeneral" type="IDENTIFIABLE">
                                <a:a name="DESC" value="EN: General configuration "/>
                                <a:a name="UUID" value="ECUC:c03229fe-4dca-416e-a47c-1ae11e6c1842"/>
                                <v:var name="DmaDevErrorDetect" type="BOOLEAN">
                                    <!-- [cover parentID={FF5660EB-3D7B-4dba-87D6-82D0D42AC135}] DmaDevErrorDetect [/cover] -->
                                    <a:a name="DESC" value="EN: Enables/Disables the Development Error Detection in the DMA Driver"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="DCE:a4e850e4-be6b-4d72-b360-c13bb0bff7f6"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>
                                <v:var name="DmaMultiCoreErrorDetect" type="BOOLEAN">
                                    <a:a name="DESC" value="If enabled, the multicore error detection will be enabled"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                      type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="UUID" 
                                       value="ECUC:348d2d10-2ba2-4a1e-aaa8-002da540f627"/>
                                    <a:da name="DEFAULT" value="true"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(../DmaDevErrorDetect = 'false') and (. = 'true')" true="EN: DMA - Invalid Configuration, Multi-Core Error Detect must be enabled only when Dev Error Detect is enabled "/>
                                    </a:da>
                                </v:var>
                                <v:var name="DmaSafetyEnable" type="BOOLEAN">
                                    <a:a name="DESC" value="EN: Enables/Disables the Safety checks in the DMA Driver"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="DCE:a4e850e4-be6b-4d72-b360-c23bb0bff7f6"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>
                                <v:var name="DmaInitCheckApi" type="BOOLEAN">
                                    <a:a name="DESC" value="EN: This parameter decides the availability of the API Dma_InitCheck in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="DCE:a4e850e4-be6b-4d72-b360-c24bb0bff7f6"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(../DmaSafetyEnable = 'true') and (. = 'false')" true="EN: DMA - Invalid Configuration, InitCheck API should be enabled when safety is enabled"/>
                                    </a:da>
                                </v:var>
                                <v:var name="DmaDeinitApiConfiguration" type="BOOLEAN">
                                    <a:a name="DESC" value="EN: This parameter decides the availability of the API Dma_ChDeinit in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-5dca-416e-a47c-1ae11e6c1852"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaSuspendApiConfiguration" type="BOOLEAN">
                                    <a:a name="DESC" value="EN: This parameter decides the availability of the API Dma_TransferFreeze and Dma_TransferResume in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-6dca-416e-a47c-1aa11e6c1862"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaTriggerApiConfiguration" type="BOOLEAN">
                                    <a:a name="DESC" value="EN: This parameter decides the availability of the API Dma_EnableHardwareTrigger and Dma_DisableHardwareTrigger in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-7dca-416e-a47c-1aa11e6c1972"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaDataPendingApiConfiguration" type="BOOLEAN">
                                    <a:a name="DESC" 
                                        value="EN: This parameter decides the availability of the API API Dma_ChGetRemainingData in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-8dca-416e-a47c-1aa11e6c2082"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaBufferSwitchApiConfiguration" type="BOOLEAN">
                                    <a:a name="DESC" 
                                        value="EN: This parameter decides the availability of the API Dma_ChSwitchBuffer in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-9dca-416e-a47c-1aa11e6c2192"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaVersionInfoApi" type="BOOLEAN">
                                    <!-- [cover parentID={4EBB01DF-10EB-46d3-9D30-37DBA42D9A3A}] DmaVersionInfoApi [/cover] -->
                                    <a:a name="DESC" 
                                        value="EN: This parameter decides the availability of the API Dma_GetVersionInfo in the code"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-9dca-416e-a48c-1aa11e6c2192"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="DmaMaxTransactionSetPerChannel" type="INTEGER">
                                    <a:a name="DESC" value="EN: Specifies how many transaction sets are allowed to be defined per channel"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-1dca-417e-a47c-1aa11e6c2212"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=65535"/>
                                        <a:tst expr="&gt;=1"/>
                                    </a:da>
                                    <a:da name="DEFAULT" value="1"/>
                                </v:var>
                                <v:var name="DmaInitApiMode" type="ENUMERATION">
                                    <!-- [cover parentID={9D70805C-6D8C-47cb-BDD3-D3B8E46C0BF4}] DmaInitApiMode [/cover] -->
                                    <!-- [cover parentID={1A65EADD-AFD0-4845-B2D2-8257E086DD67} ] User Mode [/cover] -->
                                    <!-- [cover parentID={8C2CC669-6028-49bd-9C40-C48B78F89F90} ] DMA: User Mode Support [/cover] -->
                                    <a:da name="DESC" value="This option specifies the mode in which the DMA init function operates"/>
                                    <a:da name="RANGE">
                                        <a:v>DMA_MCAL_USER1MODE</a:v>
                                        <a:v>DMA_MCAL_SUPERVISORMODE</a:v>
                                    </a:da>
                                    <a:da name="DEFAULT" value="DMA_MCAL_SUPERVISORMODE"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="((../DmaRuntimeApiMode = 'DMA_MCAL_SUPERVISORMODE') and (../DmaInitApiMode = 'DMA_MCAL_USER1MODE'))"
                                        true="If Runtime APIs are configured for Supervisory mode, the Initialization and De-Initialization APIs should also be configured in Supervisory mode."/>
                                    </a:da>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-6dca-425e-a47c-1ae21a6c2362"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                </v:var>
                                <v:var name="DmaRuntimeApiMode" type="ENUMERATION">
                                    <!-- [cover parentID={41F54C6A-2B06-41be-87E8-2B23AF914732}] DmaRuntimeApiMode [/cover] -->
                                    <a:da name="DESC" value="This option specifies the mode in which the DMA runtime functions operate"/>
                                    <a:da name="RANGE">
                                        <a:v>DMA_MCAL_USER1MODE</a:v>
                                        <a:v>DMA_MCAL_SUPERVISORMODE</a:v>
                                    </a:da>
                                    <a:da name="DEFAULT" value="DMA_MCAL_SUPERVISORMODE"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-6dca-425e-a47c-1ae31a6c2362"/>
                                    <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                </v:var>
                            </v:ctr>
                            <!--********************************************************-->
                            <!--************** DMA Channel configuration ***************-->
                            <!--********************************************************-->
                            <v:lst name="DmaChannelConfig" type="MAP">
                                <!-- [cover parentID={2DB212AD-7305-4e35-9099-DAD4BCC247D6}] DmaChannelConfig [/cover] -->
                                <a:da name="MIN" value="1"/>
                                <a:da name="MAX" type="XPath" expr="ecu:get('Dma.MaxDmaChannel')"/>
                                <v:ctr name="DmaChannelConfig" type="IDENTIFIABLE">
                                    <a:a name="DESC" value="EN: DMA channel configuration for this channel"/>
                                    <a:a name="UUID" value="ECUC:c03229fe-2dca-418e-a47c-1ab11e6c1922"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                         type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <v:var name="DmaChannelId" type="INTEGER">
                                        <a:da name="DESC" value="EN: DMA channel number"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="INVALID" type="Multi">
                                            <mt:xpath expr="text:uniq(../../*/DmaChannelId,.)"  false="EN: DMA - Duplicated value, DmaChannelId must be unique across all channels."/>
                                            <mt:range>
                                                <mt:tst expr="&lt;((ecu:get('Dma.MaxDmaChannel')))"/>
                                                <mt:tst expr="&gt;=0"/>
                                            </mt:range>
                                        </a:da>
                                        <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index,'0')"/>
                                        <a:a name="UUID" value="ECUC:c03229fe-3aca-418e-a47c-1ab11e6c2032"/>
                                        <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    </v:var>
                                    <v:var name="DmaChannelAssignedPartition" type="INTEGER">
                                        <!-- [cover parentID={2A3A64E6-48BB-4f26-8F86-777B4B371E6D}] DmaChannelAssignedPartition [/cover] -->
                                        <a:da name="DESC" value="EN: Hardware partition to which this channel is allocated to"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=3"/>
                                            <a:tst expr="&gt;=0"/>
                                        </a:da>
                                        <a:da name="DEFAULT" value="0"/>
                                        <a:a name="UUID" value="ECUC:c03229fe-3dca-418e-a47c-1ab11e6c2032"/>
                                        <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    </v:var>
                                    <v:var name="DmaChannelNumTransactionSet" type="INTEGER">
                                        <a:da name="DESC" value="EN: No of transaction set for this channel.">
                                            <a:v> EN: Linked list feature is offered in Transaction set configuration only if this parameter were enabled. The number of transaction sets is capped at DmaMaxTransactionSetPerChannel </a:v>
                                        </a:da>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="DEFAULT" value="1"/>
                                        <a:da name="INVALID" type="Multi">
                                            <mt:range>
                                                <mt:tst expr="&lt;=node:fallback(node:current()/../../../DmaGeneral/DmaMaxTransactionSetPerChannel,'65535')"/>
                                                <mt:tst expr="&gt;=1"/>
                                            </mt:range>
                                        </a:da>
                                        <a:a name="UUID" value="ECUC:c03229fe-4dca-418e-a47c-1ab11e6c2142"/>
                                        <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    </v:var>
                                    <v:var name="DmaTcsInterruptTransactionLoss" type="BOOLEAN">
                                        <!-- [cover parentID={0D376D48-5C30-4a55-B513-E55C2F2E1F39}] DmaTcsInterruptTransactionLoss [/cover] -->
                                        <a:da name="DESC" value="Enables/Disables the transaction request lost interrupt"/>
                                        <a:da name="DEFAULT" value="false"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="UUID" value="ECUC:c03229fe-5dca-426e-a47c-1ae11a6c3152"/>
                                        <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    </v:var>
                                    <v:var name="DmaChannelNotification" type="FUNCTION-NAME">
                                        <!-- [cover parentID={877896EE-4DF5-4b60-B68C-7CDB55010960}] DmaChannelNotification [/cover] -->
                                        <a:da name="DESC" value="EN: User defined Notification Function Pointer of type void NotificationRoutine(uint32_t Channel, Dma_ChEventType_t Event)" >
                                            <a:v>EN: This notification function will be called by the DMA Driver if there is a channel interrupt.</a:v>
                                        </a:da>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="(. = '')" true="EN: DMA - Invalid Configuration, Function pointer cannot be blank."/>
                                            <a:tst expr= "text:grep(., '.*\s.*')" true="EN: DMA - Invalid Configuration, Spaces not expected in the function pointer"/>
                                        </a:da>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="DEFAULT" value="NULL_PTR" />
                                        <a:a name="UUID" value="ECUC:c03229fe-5dca-418e-a47c-1ab11e6c2252"/>
                                        <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                    </v:var>
                                    <!--********************************************************-->
                                    <!--************ DMA Channel TCS configuration *************-->
                                    <!--********************************************************-->
                                    <v:lst name="DmaChannelTransactionSet" type="MAP">
                                        <a:da name="MIN" value="1"/>
                                        <a:da name="MAX" type="XPath" expr="node:fallback(node:current()/../DmaChannelNumTransactionSet,'65535')"/>
                                        <v:ctr name="DmaChannelTransactionSet" type="IDENTIFIABLE">
                                            <!-- [cover parentID={E70FDD74-A76B-4f18-8F91-C965E686621A}] DmaChannelTransactionSet [/cover] -->
                                            <a:da name="DESC" value="EN: Transaction set setting for this DMA channel"/>
                                            <a:a name="UUID" value="ECUC:c03229fe-8dca-418e-a47c-1ab11e6c2582"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                                 type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                            </a:a>
                                            <v:var name="DmaTcsIndex" type="INTEGER">
                                                <a:da name="DESC" value="EN: DMA Transaction set Index" />
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index,'0')"/>
                                                <a:da name="INVALID" type="Multi">
                                                    <mt:xpath expr="text:uniq(./../../*/DmaTcsIndex,.)" false="EN: TCS index of a DMA channel should be unique."/>
