                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-3dca-425e-a47c-1ae11a6c2032"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsSourceAddressMovement" type="ENUMERATION">
                                                <!-- [cover parentID={AF2E9280-A981-4f9d-B361-D74965678C55}] DmaTcsSourceAddressMovement [/cover] -->
                                                <a:da name="DESC" value="This parameter determines if the source address after every move operation is to be increased or decreased"/>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_INCREASING</a:v>
                                                    <a:v>DMA_DECREASING</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_INCREASING"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-4dca-425e-a47c-1ae11a6c2142"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsDestinationAddressMovement" type="ENUMERATION">
                                                <!-- [cover parentID={975346D7-DDC3-448c-BE3E-7767B6361EE8}] DmaTcsDestinationAddressMovement [/cover] -->
                                                <a:da name="DESC" value="This parameter determines if the Destinaion address after every move operation is to be increased or decreased"/>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_INCREASING</a:v>
                                                    <a:v>DMA_DECREASING</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_INCREASING"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-5dca-425e-a47c-1ae11a6c2252"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsShadowRegisterConfiguration" type="ENUMERATION">
                                                <!-- [cover parentID={5522D11D-0F1E-4bba-87B5-CC07114A0ED5}] DmaTcsShadowRegisterConfiguration [/cover] -->
                                                <a:da name="DESC" value="This parameter determines how the shadow register is to be used"/>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_SHADOWING_DISABLED</a:v>
                                                    <a:v>DMA_SOURCE_ADDRESS_BUFFERING_RO</a:v>
                                                    <a:v>DMA_DEST_ADDRESS_BUFFERING_RO</a:v>
                                                    <a:v>DMA_SOURCE_ADDRESS_BUFFERING_RW</a:v>
                                                    <a:v>DMA_DEST_ADDRESS_BUFFERING_RW</a:v>
                                                    <a:v>DMA_SOURCE_DOUBLE_BUFFERING_SW_SWITCH</a:v>
                                                    <a:v>DMA_SOURCE_DOUBLE_BUFFERING_HW_SW_SWITCH</a:v>
                                                    <a:v>DMA_DEST_DOUBLE_BUFFERING_SW_SWITCH</a:v>
                                                    <a:v>DMA_DEST_DOUBLE_BUFFERING_HW_SW_SWITCH</a:v>
                                                    <a:v>DMA_LINKED_LIST</a:v>
                                                    <a:v>DMA_ACCUMULATED_LINKED_LIST</a:v>
                                                    <a:v>DMA_SAFE_LINKED_LIST</a:v>
                                                    <a:v>DMA_CONDITIONAL_LINKED_LIST</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_SHADOWING_DISABLED"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                </a:a>
