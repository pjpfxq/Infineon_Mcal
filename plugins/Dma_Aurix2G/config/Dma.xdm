                                                <a:a name="UUID" value="ECUC:c03229fe-6dca-425e-a47c-1ae11a6c2362"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaNextTcsIndex" type="INTEGER">
                                                <!-- [cover parentID={7D6D17D7-6122-479f-A9E6-901AA0A775E3}] DmaNextTcsIndex [/cover] -->
                                                <a:da name="DESC" value="This parameter determines the next node of the TCS linked list"/>
                                                <a:da name="EDITABLE" type="XPath">
                                                    <a:tst expr="(./../DmaTcsShadowRegisterConfiguration='DMA_LINKED_LIST')
                                                            or   (./../DmaTcsShadowRegisterConfiguration='DMA_ACCUMULATED_LINKED_LIST')
                                                            or   (./../DmaTcsShadowRegisterConfiguration='DMA_SAFE_LINKED_LIST')
                                                            or   (./../DmaTcsShadowRegisterConfiguration='DMA_CONDITIONAL_LINKED_LIST')"/>
                                                </a:da>
                                                <a:da name="INVALID" type="Multi">
                                                    <mt:xpath expr="text:uniq(./../../*/DmaNextTcsIndex,.)" false="EN: DmaNextTcsIndex should be unique for each TCS."/>
                                                    <mt:xpath expr=".&lt;count(./../../*)+1" false="EN: Ponting to non-existing node"/>
                                                    <mt:range>
                                                        <mt:tst expr="&gt;=0"/>
                                                        <mt:tst expr="&lt;=65535"/>
                                                    </mt:range>
                                                </a:da>
                                                <a:da name="DEFAULT" type="XPath" expr="num:i(node:fallback(node:current()/../DmaTcsIndex,num:i(-1)) + num:i(1))"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-6dca-425e-a47c-16e11a6c2362"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsTriggerFrequency" type="ENUMERATION">
                                                <!-- [cover parentID={00111750-E2E0-45e5-B3DC-427431EB50E6}] DmaTcsTriggerFrequency [/cover] -->
                                                <a:da name="DESC">
                                                    <a:v>EN:This parameter determines how much of data will the move engine move upon a trigger request [Either hardware or software].</a:v>
                                                    <a:v>EN:The move engine upon receipt of a trigger request could effectuate a transfer or even a transaction based on the choice made.</a:v>
                                                </a:da>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_TRANSFER_PER_TRIGGER</a:v>
                                                    <a:v>DMA_TRANSACTION_PER_TRIGGER</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_TRANSFER_PER_TRIGGER"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-7dca-425e-a47c-1ae11a6c2472"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsHardwareTrigger" type="ENUMERATION">
                                                <!-- [cover parentID={66F76E29-39D6-4f92-8678-5ED0BD572CA4}] DmaTcsHardwareTrigger [/cover] -->
                                                <a:da name="DESC">
                                                    <a:v>EN:This parameter defines the triggering mode for the hardware trigger.</a:v>
                                                    <a:v>EN:DMA_NO_HARDWARE_TRIGGER- Hardware trigger is disabled</a:v>
                                                    <a:v>EN:DMA_HARDWARE_TRIGGER_SINGLE_MODE- Hardware requests are disabled by hardware after a transaction</a:v>
                                                    <a:v>EN:DMA_HARDWARE_TRIGGER_CONTINUOUS_MODE- Hardware requests are not disabled by hardware after a transaction</a:v>
                                                </a:da>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_NO_HARDWARE_TRIGGER</a:v>
                                                    <a:v>DMA_HARDWARE_TRIGGER_SINGLE_MODE</a:v>
                                                    <a:v>DMA_HARDWARE_TRIGGER_CONTINUOUS_MODE</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_NO_HARDWARE_TRIGGER"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-8dca-425e-a47c-1ae11a6c2582"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsDaisyChaining" type="BOOLEAN">
                                                <!-- [cover parentID={AA451390-8CCD-4c55-85E5-040BA4045FFC}] DmaTcsDaisyChaining [/cover] -->
                                                <a:da name="DESC">
                                                    <a:v>EN: This parameter determines whether the adjacent lower channel is daisy chained with this channel.</a:v>
                                                    <a:v>EN: If so, after a "transaction" successfully completes on this channel, an internal hardware trigger is forwarded to the adjacent channel</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-9dca-425e-a47c-1ae11a6c2692"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsInterruptSourceAddressWrap" type="BOOLEAN">
                                                <!-- [cover parentID={B3FA5C37-D543-4992-BC22-9FA5470477EE}] DmaTcsInterruptSourceAddressWrap [/cover] -->
                                                <a:da name="DESC" value="EN: Enables/Disables the wrap source interrupt"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-3dca-426e-a47c-1ae11a6c2932"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsInterruptDestinationAddressWrap" type="BOOLEAN">
                                                <!-- [cover parentID={B36DBC9D-5F6A-408a-BA33-F690BD33CCFC}] DmaTcsInterruptDestAddressWrap [/cover] -->
                                                <a:da name="DESC" value="EN: Enables/Disables the wrap destination interrupt"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-4dca-426e-a47c-1ae11a6c3042"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsDataTransferInterrupt" type="BOOLEAN">
                                                <!-- [cover parentID={B36DBC9D-5F6A-408a-BA33-F690BD33CCFC}] DmaTcsDataTransferInterrupt [/cover] -->
                                                <a:da name="DESC" value="EN: Enables/Disables the data transfer or transaction interrupt"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-4dca-416e-a47c-1ae11a6c3042"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsInterruptDataTransfer" type="ENUMERATION">
                                                <!-- [cover parentID={6C72F66E-977A-4678-9C57-E32107BA8E2B}] DmaTcsInterruptDataTransfer [/cover] -->
                                                <a:da name="DESC">
                                                    <a:v>EN: Interrupts can be generated either for every transfer or even transaction.</a:v>
                                                    <a:v>EN: Additionally, an interrupt can be generated after a certain number of transfers equaling a threshold have been completed</a:v>
                                                </a:da>
                                                <a:da name="RANGE">
                                                    <a:v>DMA_INTERRUPT_PER_TRANSFER</a:v>
                                                    <a:v>DMA_INTERRUPT_PER_TRANSACTION</a:v>
                                                    <a:v>DMA_INTERRUPT_AFTER_THRESHOLD</a:v>
                                                </a:da>
                                                <a:da name="DEFAULT" value="DMA_INTERRUPT_PER_TRANSACTION"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-1dca-426e-a47c-1ae11a6c2712"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                                <a:da name="EDITABLE" type="XPath">
                                                    <a:tst expr="node:fallback(./../DmaTcsDataTransferInterrupt ='true','true')"/>
                                                </a:da>
                                            </v:var>
                                            <v:var name="DmaTcsInterruptDataTransferThreshold" type="INTEGER">
                                                <a:da name="DESC" value="An interrupt can be generated after a certain number of transfers equaling a threshold have been completed."/>
                                                <a:da name="DEFAULT" value="0"/>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=15"/>
                                                    <a:tst expr="&gt;=0"/>
                                                </a:da>
                                                <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="(../DmaTcsInterruptDataTransfer = 'DMA_INTERRUPT_PER_TRANSACTION') and (.&gt;=1)" true="EN: DMA - Invalid Configuration, The threshold value should be zero "/>
                                                    <a:tst expr="(../DmaTcsInterruptDataTransfer = 'DMA_INTERRUPT_PER_TRANSFER') and (.&gt;=1)" true="EN: DMA - Invalid Configuration, The threshold value should be zero"/>
                                                </a:da>
                                                <a:da name="EDITABLE" type="XPath">
                                                    <a:tst expr="node:fallback(./../DmaTcsInterruptDataTransfer ='DMA_INTERRUPT_AFTER_THRESHOLD','false')"/>
                                                </a:da>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-2dca-426e-a47c-1ae11a6c2822"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsSwapDataCRCByteOrder" type="BOOLEAN">
                                            <!-- [cover parentID={6B428BE4-36D4-4b4c-A602-361A3A7B8786}] DmaTcsSwapDataCRCByteOrder [/cover] -->
                                                <a:da name="DESC" value="Swaps the Data CRC byte order"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03229fe-5dca-426e-a47c-1ae12a6c3152"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsAutoStartEnable" type="BOOLEAN">
                                                <a:da name="DESC" value="Enables the autostart feature for linked lists"/>
                                                <a:da name="DEFAULT" value="false"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:c03259fe-5dca-426e-a47c-1ae12a6c3152"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsReferenceAddressCrc" type="INTEGER">
                                                <a:da name="DESC" value="EN: Reference CRC value for address CRC" />
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:da name="DEFAULT_RADIX" value="HEX"/>
                                                <a:da name="DEFAULT" value="0"/>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=0xFFFFFFFF"/>
                                                    <a:tst expr="&gt;=0x00"/>
                                                </a:da>
                                                <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="(./../DmaTcsShadowRegisterConfiguration!='DMA_SOURCE_DOUBLE_BUFFERING_SW_SWITCH')
                                                        and   (./../DmaTcsShadowRegisterConfiguration!='DMA_SOURCE_DOUBLE_BUFFERING_HW_SW_SWITCH')
                                                        and   (./../DmaTcsShadowRegisterConfiguration!='DMA_DEST_DOUBLE_BUFFERING_SW_SWITCH')
                                                        and   (./../DmaTcsShadowRegisterConfiguration!='DMA_DEST_DOUBLE_BUFFERING_HW_SW_SWITCH')
                                                        and   (./../DmaTcsShadowRegisterConfiguration!='DMA_CONDITIONAL_LINKED_LIST')"/>
                                                </a:da>
                                                <a:a name="UUID" value="ECUC:c03929fe-9dca-418e-a47c-1ab11e6c2692"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                            <v:var name="DmaTcsReferenceDataCrc" type="INTEGER">
                                                <a:da name="DESC" value="EN: Reference CRC value for data CRC" />
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:da name="DEFAULT_RADIX" value="HEX"/>
                                                <a:da name="DEFAULT" value="0"/>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=0xFFFFFFFF"/>
                                                    <a:tst expr="&gt;=0x00"/>
                                                </a:da>
                                                <a:a name="UUID" value="ECUC:c03279fe-9dca-418e-a47c-1ab11e6c2692"/>
                                                <a:a name="ORIGIN" value="INFINEON_AURIX2G"/>
                                            </v:var>
                                        </v:ctr>
                                    </v:lst>
                                </v:ctr>
                            </v:lst>
